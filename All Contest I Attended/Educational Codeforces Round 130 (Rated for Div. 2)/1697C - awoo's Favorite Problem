#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const ll INF = 1e9 + 7;
const int SIZE = 1e5;
const int MOD = 998244353;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

void solve()
{
    int n;
    cin >> n;
    string s, t;
    cin >> s >> t;

    if (count(s.begin(), s.end(), 'b') != count(t.begin(), t.end(), 'b')) {
        cout << "NO";
        return;
    }

    int j = 0;
    bool ok = true;

    for (int i = 0; i < n; i++) {
        if (s[i] == 'b') continue;

        while (j < n && t[j] == 'b') j++;

        if (j >= n ||
            s[i] != t[j] ||
            (s[i] == 'a' && i > j) ||
            (s[i] == 'c' && i < j)) {
            ok = false;
            break;
        }
        j++;
    }

    cout << (ok ? "YES" : "NO");
}


int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    ll Test_Case;
    cin >> Test_Case;
    while (Test_Case--)
    {
    solve();
    cout << endl;
    }

    return 0;
}
