#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 1e7 + 10;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/
int a[200000];
void solve()
{

    int n;
    cin >> n;

    if (n == 1)
    {
        cout << 1 << endl;
    }
    else if (n == 2)
    {
        cout << "1 2" << endl;
    }
    else
    {
        int c = 4;
        fill(a, a + n, 0);

        a[0] = 2;
        a[n / 2] = 1;
        a[n - 1] = 3;

        for (int i = 0; i < n; i++)
        {
            if (a[i] == 0)
                a[i] = c++;
        }

        for (int i = 0; i < n; i++)
        {
            cout << a[i] << (i + 1 == n ? '\n' : ' ');
        }
    }
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    // t = 1;
    while (t--)
    {
        solve();
    }
    return 0;
}
