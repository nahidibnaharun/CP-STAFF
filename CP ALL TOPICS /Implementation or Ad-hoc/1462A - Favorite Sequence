#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
 void solve(){
int n;cin>>n;
int ar[n];
int l=0,r=n-1;
for(int i=0;i<n;i++)
    cin>>ar[i];
for(int i=0;i<n;i++){
if(i%2==0)cout<<ar[l++]<<" ";
    else cout<<ar[r--]<<" ";
}
    cout<<endl;
 }
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    int t;cin>>t;
    while(t--)
    solve();
}
