
#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 10000;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

void solve()
{

   int n, k;
    cin >> n >> k;

    map<int, set<char>> mp;

    while (k--)
    {
        string s;
        cin >> s;

        for (int i = 1; i <= n; i++)
        {
            mp[i].insert(s[i - 1]);
        }
    }

    vector<int> div;

    for (int i = 1; i * i <= n; i++)
    {
        if (n % i == 0)
        {
            div.push_back(i);
            if (i != (n / i))
                div.push_back(n / i);
        }
    }

    sort(div.begin(), div.end());

    string ans = "";
    int x = 0;

    for (auto d : div)
    {
        string res = "";

        for (int i = 1; i <= d; i++)
        {
            set<char> se(mp[i].begin(), mp[i].end());
            map<char, int> del;

            for (int j = i + d; j <= n; j += d)
            {
                for (char c : se)
                {
                    if (mp[j].find(c) == mp[j].end())
                    {
                        del[c] = 1;
                    }
                }
            }

            for (auto c : se)
            {
                if (!del[c])
                {
                    res += c;
                    break;
                }
            }
        }

        if ((int)res.size() == d)
        {
            x = d;
            ans = res;
            break;
        }
    }

    for (int i = x; i <= n; i += x)
        cout << ans;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t ;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
