void solve(){
int x,c;
cin>>x;
if(x<=1399)c=4;
else if(x<=1599)c=3;
else if(x<=1899)c=2;
else c=1;

cout<<"Division "<<c<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
    int t;cin>>t;
    while(t--)
    solve();
   return 0;   
}


