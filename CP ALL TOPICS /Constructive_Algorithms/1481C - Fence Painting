
#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 200007;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

int n, m;
int a[MAX], b[MAX], c[MAX], ans[MAX];
vector<int> g[MAX];

void solve()
{
    cin >> n >> m;

    for (int i = 1; i <= n; i++)
        g[i].clear();

    for (int i = 0; i < n; i++)
        cin >> a[i];

    for (int i = 0; i < n; i++)
    {
        cin >> b[i];
        if (a[i] != b[i])
            g[b[i]].push_back(i);
    }

    for (int i = 0; i < m; i++)
        cin >> c[i];

    int last = -1;

    if (!g[c[m - 1]].empty())
    {
        last = g[c[m - 1]].back();
        g[c[m - 1]].pop_back();
    }
    else
    {
        for (int i = 0; i < n; i++)
        {
            if (b[i] == c[m - 1])
            {
                last = i;
                break;
            }
        }
    }

    if (last == -1)
    {
        cout << "NO";
        return;
    }

    ans[m - 1] = last;

    for (int i = 0; i < m - 1; i++)
    {
        if (g[c[i]].empty())
        {
            ans[i] = last;
        }
        else
        {
            ans[i] = g[c[i]].back();
            g[c[i]].pop_back();
        }
    }

    for (int i = 1; i <= n; i++)
    {
        if (!g[i].empty())
        {
            cout << "NO";
            return;
        }
    }

    cout << "YES" << endl;
    for (int i = 0; i < m; i++)
    {
        if (i)
            cout << ' ';
        cout << ans[i] + 1;
    }
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
