int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 

    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int s=0,sm=0;
        for(int i=0;i<n;i++){
            int b;
            cin>>b;
            s+=b;
            if(b>0)sm+=(b-1);
        }
        if(s>=100 && sm <100)cout<<"YES";
        else cout<<"NO";
        cout<<endl;
    }
    return 0;   
}
