#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
int n;cin>>n;
string s;cin>>s;
char ar[26]={0};
int b=0;
for(int i=0;i<n;i++){
    if(ar[s[i]-'A']==0){
        b+=2;
        
    }
    else b++;
    ar[s[i]-'A']++;
}cout<<b;
cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
    int t;cin>>t;
    while(t--)
    solve(); 
}


