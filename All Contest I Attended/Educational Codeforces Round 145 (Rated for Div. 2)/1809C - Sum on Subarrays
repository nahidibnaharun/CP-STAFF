#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 10000;
const ll MAXN = 2e5 + 5;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

vector<ll> fun(ll n, ll k)
{
    if (n == 0)
        return {};

    if (k < n)
    {
        vector<ll> a(n, -1);
        if (k > 0)
            a[k - 1] = 200;
        a[k] = -400;
        return a;
    }
    else
    {
        vector<ll> a = fun(n - 1, k - n);
        a.push_back(1000);
        return a;
    }
}

void solve()
{
    ll n, k;
    cin >> n >> k;

    vector<ll> b = fun(n, k);

    for (auto x : b)
        cout << x << " ";
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
