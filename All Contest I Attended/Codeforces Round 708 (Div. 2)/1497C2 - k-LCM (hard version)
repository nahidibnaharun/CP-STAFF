vector<int> solve3(int n)
{
    if (n & 1)
        return {1, n / 2, n / 2};
    if (n % 4 == 0)
        return {n / 2, n / 4, n / 4};
    return {2, n / 2 - 1, n / 2 - 1};
}

void solve()
{
    int n, k;
    cin >> n >> k;

    vector<int> added = solve3(n - k + 3);

    for (int i = 0; i < k; i++)
    {
        if (i < 3)
            cout << added[i] << " ";
        else
            cout << 1 << " ";
    }
    cout << endl;
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
    }
    return 0;
}
