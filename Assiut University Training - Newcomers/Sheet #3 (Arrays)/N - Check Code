void solve(){
int a,b;cin>>a>>b;
string s;cin>>s;
for(int i=0;i<a;i++){
  int x=s[i]-'0';
  if(!(x<10 && x>=0)){
    cout<<"No";return;
  }
}
for(int i=a+1;i<(a+b);i++){
  int x=s[i]-'0';
  if(!(x<10 && x>=0)){
    cout<<"No";return;
  }
}
if(s[a]=='-')
cout<<"Yes";
else cout<<"No";
}
    
int main(){
    ios_base::sync_with_stdio(false);
