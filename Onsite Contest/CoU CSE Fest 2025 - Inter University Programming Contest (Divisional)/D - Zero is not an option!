#include<bits/stdc++.h>
using namespace std;
using lli = long long int;
using ll = long long;
using ull = unsigned long long;
const ll MOD = 998244353;
const ll INF = 2000000007LL;

/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
  int n,m;cin>>n>>m;
  vector<int>cnt(60,0);
  for(int i=0;i<n;i++){
    vector<char>rw(60,0);
    for(int j=0;j<m;j++){
        ll x;cin>>x;
        for(int l=0;l<60;l++){
            if((x>>l)&1LL)rw[l]=1;
        }
    }
    for(int l=0;l<60;l++)if(rw[l])++cnt[l];
  }
bool ch=false;
for(int i=0;i<60;i++){
    if(cnt[i]==n){ch=true;break;}
}
cout<<(ch?"YES":"NO");
}
     
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--){
  solve();
  cout<<endl;
  }
}
