#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int INF = 1e9;
const int MOD = 1000000007;
/*
//     
         _   _  __    _   _  _____   _____
//     / | / / / _ \ \ \__\ \\__  _\\  _   \ 
//    /  |/ / / /_\ \ \  __  \  \ \  \ \ \  \ 
//   / /|  / / ____  \ \ \  \ _\_\ \__\ \/  /
//  /_/ |_/ /_/     \__\\_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve() {
    ll n,x,y;
    cin >>n>>x>>y;
    if(n==2){
        cout<<x<<" "<<y;
        return;
    }
    ll df=y-x;

    ll nn=n-1;
    if(df%nn==0){
        ll d=df/nn;
        for(int i=0;i<n;i++){
            cout<<x<<" ";
            x+=d;
        }
    }
    else{
        while(df%nn!=0){
            nn--;
        }
        ll d=df/nn;
        ll nx=x,tl=0;
        while(nx<=y){
            cout<<nx<<" ";
            nx+=d;
            tl++;
        }

        nx=x; nx-=d;
        while(nx>0 && tl<n){
            cout<<nx<<" ";
            tl++;
            nx-=d;
        }
        if(tl==n)return;
        nx=y+d;
        while(tl<n){
            cout<<nx<<" ";
            nx+=d;
            tl++;
        }
    }

}

// 20 40 30 50 10
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   
  int t; cin >>t;
  while(t--){
    solve();
    cout<<endl;
  }
return 0;
}
