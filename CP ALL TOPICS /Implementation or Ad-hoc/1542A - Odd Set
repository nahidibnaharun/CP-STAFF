 void solve(){
int n,cnt[2]={0};
cin>>n;
for(int i=1,x;i<=n*2;i++)cin>>x,cnt[x%2]++;
   if(cnt[0]==n)cout<<"Yes";
else cout<<"No";
cout<<endl;
 }
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    ll t;cin>>t;
    while(t--)
    solve();


       
}
