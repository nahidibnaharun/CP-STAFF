#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const int N = 1e3 + 10;
const ll M = 1e9 + 10;

/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
int n;cin>>n;vector<int>a(n);
for(auto &x:a)cin>>x;
int mp=0;
for(int j=0;j<=100;j++){
    int ts=0;
    for(int i=0;i<n;i++)ts+=min(j,a[i]);
    int pt=ts*50-j*30*n;mp=max(mp,pt);
}
cout<<mp;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--){
    solve();
    cout<<endl;
  }
} 
