#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 10000;
const ll MAXN = 2e5 + 5;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

void solve()
{  int n, m;
    cin >> n >> m;

    vector<vector<int>> a(n, vector<int>(m));
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < m; j++)
        {
            cin >> a[i][j];
        }
    }

    vector<int> bad;

    for (int i = 0; i < n && bad.empty(); i++)
    {
        vector<int> b = a[i];
        sort(b.begin(), b.end());

        for (int j = 0; j < m; j++)
        {
            if (a[i][j] != b[j])
                bad.push_back(j);
        }
    }

    if ((int)bad.size() == 0)
    {
        cout << 1 << " " << 1;
        return;
    }

    if ((int)bad.size() > 2)
    {
        cout << -1;
        return;
    }

    for (int i = 0; i < n; i++)
    {
        swap(a[i][bad[0]], a[i][bad[1]]);
    }

    for (int i = 0; i < n; i++)
    {
        for (int j = 1; j < m; j++)
        {
            if (a[i][j] < a[i][j - 1])
            {
                cout << -1;
                return;
            }
        }
    }

    cout << bad[0] + 1 << " " << bad[1] + 1;
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
