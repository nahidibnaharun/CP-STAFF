#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int MOD = 676767677LL;
const int MAX = 1e7 + 5;
/*
//      _   _  __  _   _
//     / | / / / _ \ \ \__\ \\__  _\\  _  \ 
//    /  |/ / / /_\ \ \  _ \  \ \   \ \ \  \ 
//   / /|  / / __  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

