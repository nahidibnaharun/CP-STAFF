#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const int INF=500;
const ll MOD = 1000000007;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve() {
    int n;
    cin>>n;
    vector<char>ok1(n/2,0),ok2(n/2,0);
    for(int i=0;i<2;++i){
        string s;
        cin>>s;
        for(int j=0;j<n;++j){
            if((i+j)&1){
                ok1[(i+j)/2]|=(s[j]=='>');
               ok2[(j-i+1)/2]|=(s[j]=='>');
            }
        }
    }
    bool ans = true;
    for (int i=0;i<n/2;++i){
        if (!(ok1[i] && ok2[i])) {
            ans = false;
            break;
        }
    }
    cout << (ans ? "YES" : "NO");
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
    solve();
    cout<<endl;
    }
}
