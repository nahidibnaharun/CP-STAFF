LANG: C++
TASK: practice 
*/

        
void solve(){
 ll a, b, c;
        cin >> a >> b >> c;
        for (int i = 0; i < 5; i++) {
            if (a <= b && a <= c)
                ++a;
             else if (b <= a && b <= c)
                ++b;
             else
                ++c;
            
        }
        cout << (a * b * c) <<endl;
}


int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    ll t;cin>>t;
    while(t--)
    solve();    
}
