void solve()
{

   string s;
    cin >> s;
    int cnt=0;
    for(auto &x:s){
        if(x=='Y')cnt++;
    }
    if (cnt > 1) cout << "NO";
    else cout << "YES";
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    // seive();
    ll Test_Case;
    cin >> Test_Case;
    while (Test_Case--)
    {
        solve();
        cout << endl;
    }

    return 0;
}
