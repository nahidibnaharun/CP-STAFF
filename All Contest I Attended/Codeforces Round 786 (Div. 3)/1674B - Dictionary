void solve(){
    string s="aa";
    map<string, int>idx;
    int i=1;
    for(s[0] = 'a' ; s[0] <= 'z' ; s[0]++){
        for(s[1] = 'a' ; s[1] <= 'z' ; s[1]++){
            if(s[0] != s[1])idx[s]=i++;
        }
    }
    int t; cin >>t;
    while(t--){
        string x;cin >>x;
        cout<<idx[x]<<endl;
    }
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int Test_Case;
    // while(Test_Case--){
        solve();
    // }
    return 0;
}
