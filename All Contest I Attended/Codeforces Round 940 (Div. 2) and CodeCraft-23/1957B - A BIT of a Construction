
#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 200007;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

void solve()
{
    int n, k;
    cin >> n >> k;

    vector<int> a(n);

    if (n == 1)
    {
        a[0] = k;
    }
    else
    {
        int msb = 0;
        for (int i = 0; i < 31; i++)
        {
            if (k & (1 << i))
                msb = i;
        }

        a[0] = (1 << msb) - 1;
        a[1] = k - a[0];

        for (int i = 2; i < n; i++)
            a[i] = 0;
    }

    for (int i = 0; i < n; i++)
        cout << a[i] << " ";
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
