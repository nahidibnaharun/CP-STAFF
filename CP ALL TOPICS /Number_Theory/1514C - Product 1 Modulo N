
#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 10000;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

void solve()
{

    ll n;
    cin >> n;

    vector<ll> v;
    ll p = 1;

    for (ll i = 1; i < n; i++)
    {
        if (__gcd(i, n) == 1)
        {
            v.push_back(i);
            p = (p * i) % n;
        }
    }

    if (p == 1)
    {
        cout << v.size() << endl;
        for (auto i : v)
            cout << i << " ";
    }
    else
    {
        cout << v.size() - 1 << endl;
        for (auto i : v)
            if (i != p)
                cout << i << " ";
    }
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t = 1;
    // cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
