void solve(){
    int n;cin>>n;
    n--;
    ll x=1;
    for(int i=1;i<=n;i++){
        x*=i;
    }
    cout<<x;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--){
  solve();
  cout<<endl;
  }
}
