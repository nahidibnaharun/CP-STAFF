#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int INF = 1e9;
const int MOD = 1000000007;
/*
//     
         _   _  __    _   _  _____   _____
//     / | / / / _ \ \ \__\ \\__  _\\  _   \ 
//    /  |/ / / /_\ \ \  __  \  \ \  \ \ \  \ 
//   / /|  / / ____  \ \ \  \ _\_\ \__\ \/  /
//  /_/ |_/ /_/     \__\\_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

void solve(){
    
   ll n; cin >> n;
        vector<ll> v;
        for (ll i = 0; i < 2 * n; i++) {
            ll x; cin >> x; v.push_back(x);
        }
 
        vector<ll> l(n + 3, 0);
        for (ll i = 0; i < 2 * n; i += 2) l[1] -= v[i];
        for (ll i = 1; i < 2 * n; i += 2) {
            l[1] += v[i]; l[2] -= v[i];
        }
        for (ll i = 2; i < 2 * n; i += 2) l[2] += v[i];
        l[2] += 2 * v.back() - v[0];
 
        for (ll i = 3; i <= n; i++) {
            l[i] = l[i - 2] + 2 * (v[(ll)v.size() - i + 1] - v[i - 2]);
        }
 
        for (ll i = 1; i <= n; i++) cout << l[i] << ' ';
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   
   int t;
   cin >>t;
   while(t--){
    solve();
    cout<<endl;
   }
return 0;
}
