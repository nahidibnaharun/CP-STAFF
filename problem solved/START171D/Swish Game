}

int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int t;
	cin>>t;
	while(t--){
		int a,b,sw=0,pas=0;
		cin>>a>>b;
		string s;
		cin>>s;
		for(int i=0;i<s.length();i++){
		    if(s[i]=='S')sw++;
		    else pas++;
		}
		if(sw>=b)cout<<a<<endl;
		else{
		    cout<<b-sw-1+a<<endl;
		}
	}

}
