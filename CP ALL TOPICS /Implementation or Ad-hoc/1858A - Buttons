 void solve(){
ll a,b,c;cin>>a>>b>>c;
if(c%2==0){
   if(a>b)cout<<"First";
   else cout<<"Second";
}
else{
   if(b>a)cout<<"Second";
   else cout<<"First";
}
cout<<endl;
 }
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    ll t;cin>>t;
    while(t--)
    solve();


       
}
