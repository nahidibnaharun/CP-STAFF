void solve() {
    int n;cin >>n;
    string s;cin >>s;
    vector<int> pos;
    for(int i=0 ; i<n ;i++){
        if(s[i] =='0')pos.push_back(i+1);
    }
    cout << (int) pos.size() <<endl;
    for(auto &x : pos)cout<<x<<" ";


}

int main(){
ios_base::sync_with_stdio(false);
cin.tie(NULL);
    int Test_Case;
    cin >> Test_Case;
    
    while(Test_Case--){
        solve();
        cout<<endl;
    }
return 0;
}
    
