void solve(){
int p,t;
cin>>p>>t;
t=240-t;
int c=0,a=0;
for(int i=1;i<=p;i++){
    c+=5*i;
    if(c<=t)a++;
    else{
        break;
    }
}
cout<<a;
cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
    // int t;cin>>t;
    // while(t--)
    solve();
   return 0;   
}


