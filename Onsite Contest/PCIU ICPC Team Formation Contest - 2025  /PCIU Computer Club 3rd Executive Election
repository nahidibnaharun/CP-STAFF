void solve() {
string s;
cin >>s;
int a=0,b=0,c=0;
for(auto x:s){
    if(x=='A')a++;
    else if(x=='B')b++;
    else if(x=='C')c++;
}
if(a>b && a>c){
    cout<<"Alice";
}
else if(b>c && b>a){
    cout<<"Bob";
}
else if(c>a && c>b){
    cout<<"Charlie";
}
else cout<<"Audience Confused!";
cout<<endl;
}

