void solve() {
    string t; cin >> t;
    if(t=="first"){
        int n; cin >> n;
        for(int i=0 ; i<n ;i++){
            int x; cin >>x ; cout << char(x+'a'-1);
        }
    }
    else{
        string s; cin >>s;
        cout<<s.size() <<" ";
        for(char x:s)cout<<x-'a'+1<<" ";
    }

}


