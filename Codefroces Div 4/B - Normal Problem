#include <bits/stdc++.h>

using namespace std;

using ll = long long;

/*
ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    int n;
    cin>>n;
    while(n--){
        string s;
        cin>>s;
        for(int i=0;i<s.length();i++){
            if(s[i]=='p')s[i]='q';
            else if(s[i]=='q')s[i]='p';
        }
        for(int i=s.length()-1;i>-1;i--){
            cout<<s[i];
        }
        cout<<endl;
    }
   return 0;
    
}
