int mfun(vector<int>ar, int n){
    int me=INT_MIN;
    for(auto x:ar)me=max(me,x);
    return me;
}
int mifun(vector<int>ar, int n){
    int me=INT_MAX;
    for(auto x:ar)me=min(me,x);
    return me;
}
void solve(){
int n;cin>>n;
vector<int>ar(n);
for(auto &x:ar)cin>>x;
cout<<mifun(ar,n)<<" "<<mfun(ar,n);
}


