void solve(){
int n;cin>>n;
vector<ll>a(n);for(auto &x:a)cin>>x;
for(int i=0;i<n;i++){
    if(a[i]==0){
        reverse(a.begin(),a.begin()+i);
    }
}
for(auto &x:a)cout<<x<<" ";
}
    
