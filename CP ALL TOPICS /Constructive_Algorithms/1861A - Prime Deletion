void solve()
{
    string s;
    cin >> s;

    if (s.find('1') < s.find('3'))
        cout << 13;
    else
        cout << 31;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
