#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const ll INF = 1e17;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

void solve(){
ll n;cin>>n;
vector<ll>ans;
ll tn=1;
for(int i=1;i<=18;++i){
    tn*=10;
    ll dm=tn+1;
    if(dm>n)break;
    if(n%dm==0)ans.push_back(n/dm);
}
if(ans.empty())cout<<0;
else{
    reverse(ans.begin(),ans.end());
    cout<<ans.size()<<endl;
    for(auto x:ans)cout<<x<<" ";
}
 }
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
