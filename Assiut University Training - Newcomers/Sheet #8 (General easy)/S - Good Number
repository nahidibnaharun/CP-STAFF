void solve(){
int n,k;cin>>n>>k;
int cnt=0;
while(n--){
    string s;cin>>s;
    vector<int>ar(k+1,0);
    for(auto x:s){
        int dg=x-'0';
        if(dg>=0 && dg<=k)
        ar[dg]++;
    }
    bool x=true;
    for(int i=0;i<=k;i++){
        if(ar[i]==0){x=false;break;}
    }
    if(x)cnt++;
}cout<<cnt;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // ll t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
return 0;
}
