#include<bits/stdc++.h>
using namespace std;
using ll = long long;
const int INF = 1e9;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

void solve(){
   int n; cin >> n; vector<string> s(n);
   for(auto &x : s) cin >> x;

    int ms = INF ,mxs = -INF , md = INF , mxd = -INF , mi = INF  , mxi = -INF , mj = INF , mxj = -INF ;

    for(int i = 0 ; i<n ;i++){
        for(int j=0 ;j<n ;j++){
            if(s[i][j]=='#'){
                ms=min(ms,i+j);
                mxs = max(mxs,i+j);
                md= min(md,i-j);
                mxd=max(mxd,i-j);
                mi = min(mi,i);
                mxi = max(mxi, i);
                mj= min(mj,j);
                mxj= max(mxj,j);
            }
        }
    }

    if((mxi - mi <=1  && mxj -mj <=1) || mxs - ms <=1 || mxd - md <=1)
        cout <<"YES";
    else cout<<"NO";
   
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    
    ll Test_Case; cin >> Test_Case;
    while(Test_Case--){
        solve();
        cout << endl;
    }
    
    
    return 0;   
}
