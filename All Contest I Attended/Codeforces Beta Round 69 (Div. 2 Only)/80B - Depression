void solve(){
    string s;cin>>s;
    int h= (s[0]-'0')*10 + (s[1]-'0'),m=(s[3]-'0')*10 + (s[4]-'0');
    h%=12;
    double ha=h*30.0+m*0.5,ma=m*6.0;
    cout<<fixed<<setprecision(10)<<ha<<" "<<ma;

}
