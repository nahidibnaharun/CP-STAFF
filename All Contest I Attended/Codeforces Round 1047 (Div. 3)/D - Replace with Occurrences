#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const ll INF = 1e12+7;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

void solve(){
    int n;cin>>n;
    vector<int>b(n+1);
    for(int i=1;i<=n;i++)cin>>b[i];
    vector<vector<int>>ps(n+1);
    for(int i=1;i<=n;i++)ps[b[i]].push_back(i);
    bool chk=true;
    int lv=1;
    vector<int>a(n+1,0);
    for(int i=1;i<=n && chk ;i++){
        int m=ps[i].size();
        if(m==0)continue;
        if(m%i !=0){chk=false;break;}
        for(int j=0;j<m;j+=i){
            for(int k=0;k<i;k++){
                a[ps[i][j+k]]=lv;
            }
            ++lv;
        }
        }
        if(!chk){cout<<-1;}
        else{
            for(int j=1;j<=n;++j){
                if(j==n)cout<<a[j];
                else cout<<a[j]<<" ";
            }
        }
}
    
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
