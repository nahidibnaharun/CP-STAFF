 void solve(){
 
 ll n;cin>>n;
 ll c1=n/3,c2=c1;
 if(n%3==1)c1+=1;
 else if(n%3==2)c2+=1;
 cout<<c1<<" "<<c2;
    cout<<endl;
 }
