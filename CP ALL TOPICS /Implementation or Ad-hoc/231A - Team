#include <bits/stdc++.h>

using namespace std;

using ll = long long;

/*
ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    int t,ans=0;cin>>t;
   
    for(int i=0;i<t;i++){
        int a,b,c;cin>>a>>b>>c;

        if(a+b+c>1)ans++;
    }
    cout<<ans;
   return 0;
    
}
