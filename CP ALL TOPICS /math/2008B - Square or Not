#include<bits/stdc++.h>
using namespace std;
using lli = long long int;
using ll = long long;
using ull = unsigned long long;
const ll MOD = 998244353;
const ll INF = 2000000007LL;

/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
    int n;cin>>n;string s;cin>>s;
    int id=0;
    while(id<n && s[id]=='1')id++;
    if(id==n){
        if(n==4)cout<<"YES";
        else cout<<"NO";
    }
    else{
        if((id-1)*(id-1)==n)cout<<"YES";
        else cout<<"NO";
    }
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--){
  solve();
  cout<<endl;
  }
}
