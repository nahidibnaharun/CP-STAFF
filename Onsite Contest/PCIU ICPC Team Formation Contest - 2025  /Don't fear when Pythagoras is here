void solve() {

    int a,b,c;
    while(cin >>a>>b>>c){
    if(a==0 && b==0 && c==0){
        cout<<-1;
    }
    else{
         ll d=a*a,e=b*b , f=c*c;
         if(d==(e+f) || (d+f)==e || (d+e)==f)cout<<"Pythagoras";
         else{
            cout<<a+b+c;
         }
    }

    cout<<endl;
}

}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

//  int t; cin >>t;
 // while(t--){
    solve();
   // cout<<endl;
  //}
return 0;
}
