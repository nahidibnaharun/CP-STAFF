void solve(){
string s;
cin>>s;
if(s.size()==2)cout<<s;
else{
    int n=s.size();
    cout<<s[0];
    for(int i=1;i<n;i+=2)cout<<s[i];
}
cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
    int t;cin>>t;
    while(t--)
    solve();
   return 0;   
}


