
#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 10000;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

void solve()
{
    ll n;
    string s;
    cin >> n >> s;

    ll ans = 0;

    for (ll i = 0; i < s.size(); i++)
    {
        int fr[10] = {}, ds = 0, mfre = 0;

        for (ll j = i; j < s.size() && (++fr[s[j] - '0']) <= 10; j++)
        {
            mfre = max(mfre, fr[s[j] - '0']);

            if (fr[s[j] - '0'] == 1)
                ds++;

            if (ds >= mfre)
                ans++;
        }
    }

    cout << ans;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
