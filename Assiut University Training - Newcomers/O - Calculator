void solve(){
string s;cin>>s;
int n=s.size(),x;
for(int i=0;i<n;i++){
    if(s[i]=='+' || s[i]=='-' || s[i]=='/' ||s[i]=='*'){
        x=i;break;
    }
}
string s1=s.substr(0,x),s2=s.substr(x+1,n-x-1);
int n1=s1.size(),n2=s2.size();
int nm1=0,nm2=0;
for(int i=0;i<n1;i++){
    nm1+=(s1[i]-'0')*pow(10,n1-i-1);
}
for(int i=0;i<n2;i++){
    nm2+=(s2[i]-'0')*pow(10,n2-i-1);
}
// cout<<nm1<<" "<<nm2<<endl;
if(s[x]=='*')cout<<nm1*nm2;
else if(s[x]=='/')cout<<nm1/nm2;
else if(s[x]=='+')cout<<nm1+nm2;
else cout<<nm1-nm2;
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
return 0;
}
