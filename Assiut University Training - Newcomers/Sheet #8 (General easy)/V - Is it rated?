void solve(){
int n;cin>>n;
vector<pair<int,int>>a(n);
bool rat=true;
for(auto &x:a){
    cin>>x.first>>x.second;
    if(x.first!=x.second)rat=false;
}
if(rat){
    for(int i=0;i<n-1;i++){
        if(a[i].first<a[i+1].second){
            cout<<"unrated";return;
        }
    }
    cout<<"maybe";
}
else{
    cout<<"rated";
}

}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // ll t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
return 0;
}
