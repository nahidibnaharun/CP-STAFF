void solve(){
    ll n;cin>>n;
    ll no1=__builtin_popcountll(n);
    if(no1)cout<<(1LL<<no1)-1;
    else cout<<0;
    cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
//   int t;cin>>t;
//   while(t--)
    solve();
} 
