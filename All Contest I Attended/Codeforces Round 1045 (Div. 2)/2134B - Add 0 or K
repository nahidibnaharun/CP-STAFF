#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const ll INF = 1e17;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
ll modl(ll a,ll b,ll m){
    ll x=(ll)a*b;
    return (ll)(x%m);
}
ll modp(ll b,ll ex,ll m){
    ll r=1%m;b%=m;
    while(ex){
        if(ex&1)r=modl(r,b,m);
        b=modl(b,b,m);
        ex>>=1;
    }return r;
}
void solve(){
   int n; ll k;cin>>n>>k;
   vector<ll>a(n); for(auto &x:a)cin>>x;
   ll gc=a[0];
   for(int i=1;i<n;++i)gc=gcd(gc,a[i]);
   if(gc>1){
    for(int i=0;i<n;++i){ cout<<a[i]<<" ";}return;
   }
   ll x=k+1, p=-1;
   for(ll d=2 ;d*d<=x ;++d)if(x%d==0){p=d; break;}
   if(p==-1)p=x;
   ll km=k%p,inv=modp(km,p-2,p);
   for(int i=0;i<n;++i){
    ll rm=a[i]%p;if(rm<0)rm+=p;
    ll nd=(p-rm)%p;
    ll r=modl(nd,inv,p);
    ll vl=(ll)a[i]+(ll)r*(ll)k;
    ll ans=(ll)vl;
    cout<<ans<<" ";
   }
 }

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
