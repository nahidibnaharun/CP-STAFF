void solve(){
int t;cin>>t;
priority_queue<int,vector<int>,greater<int>>pq;
while(t--){
    int ty;cin>>ty;
    if(ty==1){int x;cin>>x;pq.push(x);}
    else{cout<<pq.top()<<endl;pq.pop();}
}
}
