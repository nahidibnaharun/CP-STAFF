#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const int N = 1e3 + 10;
const ll M = 1e9 + 10;

/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
    int n,k;cin>>k>>n;
    int j=1,df=1;
    for(int i=1;i<=k;i++){
        cout<<j<<" ";
        if((n-(j+df))>=(k-(i+1))){j+=df;df++;}
        else j++;
    }
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--){
    solve();
    cout<<endl;
  }
} 
