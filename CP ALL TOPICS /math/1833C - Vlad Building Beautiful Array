#include<iostream>
#include<vector>
#include<algorithm>
int main(){
    std::ios_base::sync_with_stdio(false);
    std::cin.tie(NULL);
    int t;
    if(!(std::cin>>t)) return 0;
    while(t--){
        int n;
        std::cin>>n;
        std::vector<int> a(n);
        int m=2e9;
        for(int i=0;i<n;++i){
            std::cin>>a[i];
            if(a[i]<m) m=a[i];
        }
        if(m%2!=0){
            std::cout<<"YES"<<"\n";
        }else{
            bool ok=true;
            for(int i=0;i<n;++i){
                if(a[i]%2!=0){
                    ok=false;
                    break;
                }
            }
            if(ok) std::cout<<"YES"<<"\n";
            else std::cout<<"NO"<<"\n";
        }
    }
    return 0;
}
