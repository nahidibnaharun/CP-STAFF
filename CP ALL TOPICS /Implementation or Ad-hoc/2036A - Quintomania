void solve(){
int n;cin>>n;
int ar[n];for(auto &x:ar)cin>>x;
for(int i=0;i<n-1;i++){
    int df=abs(ar[i]-ar[i+1]);
    if(df!=5 && df!=7){
        cout<<"NO"<<endl;return;
    }
}cout<<"YES";
    cout<<endl;
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
  int t;cin>>t;
  while(t--)
    solve();
} 
