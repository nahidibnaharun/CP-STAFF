bool fun(int n,int m){
    if(n==m)return true;
    else if(n%3!=0)return false;
    else {return (fun(n/3,m) || fun(2*n/3,m));}
}
void solve(){
int n,m;cin>>n>>m;
cout<<(fun(n,m)?"YES":"NO");
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }    
return 0;
}
