void solve(){
string s;getline(cin,s);
int qpos = s.find('?');
string query = s.substr(qpos + 1);
size_t pos = 0;

for(int i = 0 ; i<5 ;++i){
    size_t eq = query.find('=',pos);
    size_t amp = query.find('&',eq);
    string name = query.substr(pos , eq - pos);
    string value; 
    if(amp ==  string :: npos) value = query.substr(eq + 1);
    else value = query.substr(eq+1,amp-eq-1);
    cout<<name<<": "<<value<<endl;
    pos = amp+1;

}

}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
