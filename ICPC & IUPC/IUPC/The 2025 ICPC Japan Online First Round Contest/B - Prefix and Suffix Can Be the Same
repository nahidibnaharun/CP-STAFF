vector<int> prefix(string &s){
    int n = (int)s.size();

    vector<int>pi(n,0);

    for(int i=1 ;i<n;i++){
        int j= pi[i-1];

        while(j>0 && s[i] !=s[j])j=pi[j-1];

        if(s[i]==s[j])j++;
        pi[i]=j;
    }
    return pi;
}

void solve(){
    while(true){
        ll n; cin >> n;
        if(n==0)return;

        string s; cin >>s;

        vector<int>pi = prefix(s);

        int p = pi.back();
        cout<<s<<s.substr(p);
        cout<<endl;
    }
}



int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

