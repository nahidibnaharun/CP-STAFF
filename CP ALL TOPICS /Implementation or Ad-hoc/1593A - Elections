int fun(int a,int b,int c){return max(0,max(b,c)+1-a);}
void solve(){
int a,b,c;cin>>a>>b>>c;
cout<<fun(a,b,c)<<" "<<fun(b,a,c)<<" "<<fun(c,a,b);

cout<<endl;
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   int t;cin>>t;
   while(t--)
    solve();
} 
