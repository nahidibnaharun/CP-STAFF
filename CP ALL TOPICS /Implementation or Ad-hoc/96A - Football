int main() {
    string s;
    cin >> s;

    int current_team_count = 1;
    int max_team_count = 1;
    char last_player = s[0];

    for (int i = 1; i < s.length(); i++) {
        if (s[i] == last_player) {
            current_team_count++;
        } else {
            current_team_count = 1;
            last_player = s[i];
        }

        if (current_team_count >= 7) {
            cout << "YES" << endl;
            return 0;
        }
    }

    cout << "NO" << endl;

    return 0;
}
