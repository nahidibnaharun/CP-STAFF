#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int INF = 1e9;
const int MOD = 1000000007;
/*
//     
         _   _  __    _   _  _____   _____
//     / | / / / _ \ \ \__\ \\__  _\\  _   \ 
//    /  |/ / / /_\ \ \  __  \  \ \  \ \ \  \ 
//   / /|  / / ____  \ \ \  \ _\_\ \__\ \/  /
//  /_/ |_/ /_/     \__\\_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve() {
    int n;
    cin >>n;
    vector<ll>a(n);
    for(auto &x:a)cin >>x;

    sort(a.begin(),a.end());
   
        ll mxd=INT_MAX,idx=-1;
        for(int i=0;i<n-1;i++){
            ll df=a[i+1]-a[i];
            if(mxd>df){
                idx=i;
                mxd=df;
            }
        }

        cout<<a[idx]<<" ";
        for(int i=idx+2;i<n;i++){
            cout<<a[i]<<" ";
        }
        for(int i=0;i<idx;i++)cout<<a[i]<<" ";
        cout<<a[idx+1];
   
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   
  int t; cin >>t;
  while(t--){
    solve();
    cout<<endl;
  }
return 0;
}
