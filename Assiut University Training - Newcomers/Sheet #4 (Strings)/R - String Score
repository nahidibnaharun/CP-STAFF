void solve(){
int n;cin>>n;
string s;cin>>s;
int cnt=0;
for(int i=0;i<n;i++){
    if(s[i]=='V')cnt+=5;
    else if(s[i]=='W')cnt+=2;
    else if(s[i]=='X'&& i<n-1){
        i++;
    }
    else if(s[i]=='Y' && i<n-1){
        s+=s[i+1];
        n++;
        i++;
    }
    else if(s[i]=='Z' && i<n-1){
        if(s[i+1]=='V'){
            cnt/=5;i++;
        }
        else if(s[i+1]=='W'){
            cnt/=2;i++;
        }
    }
}
cout<<cnt;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
