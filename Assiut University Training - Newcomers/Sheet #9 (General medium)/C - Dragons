void solve(){
    int s,l,cnt=1;
   cin>>s>>l;
   pair<int,int> a[l];
   for(int i=0;i<l;i++) cin>>a[i].first>>a[i].second;
   sort(a,a+l);
   for(int i=0;i<l;i++){
       if(a[i].first<s){
           s=s+a[i].second;
       }
       else{
           cnt=0;
       }
   }
   (cnt==0)?cout<<"NO":cout<<"YES";

}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
