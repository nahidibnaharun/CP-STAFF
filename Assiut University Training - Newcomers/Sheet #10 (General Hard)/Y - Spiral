#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int MOD = 676767677LL;
const int MAX = 1e7 + 5;
/*
//      _   _  __  _   _
_____
_____
//     / | / / / _ \ \ \__\ \\__  _\\  _  \ 
//    /  |/ / / /_\ \ \  _ \  \ \   \ \ \  \ 
//   / /|  / / __  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

void solve(){

int n,m;
cin >> n>>m;
vector<vector<ll>>a(n,vector<ll>(m));

for(int i=0;i<n;i++){
    for(int j=0;j<m;j++)cin>>a[i][j];
}
 int rowu = 0, rowd = n - 1, left = 0, right = m - 1;

    while (rowu <= rowd && left <= right) {  
        for (int i = left; i <= right; i++) cout << a[rowu][i] << " ";
        rowu++;

        for (int i = rowu; i <= rowd; i++) cout << a[i][right] << " ";
        right--;

        if (rowu <= rowd) { 
            for (int i = right; i >= left; i--) cout << a[rowd][i] << " ";
            rowd--;
        }

        if (left <= right) {  
            for (int i = rowd; i >= rowu; i--) cout << a[i][left] << " ";
            left++;
        }
    }

}


    

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
//    ll t;cin>>t;
//    while(t--){
        solve();
//         cout<<endl;
//    }
return 0;
}
    
