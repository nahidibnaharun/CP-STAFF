#include <bits/stdc++.h>

using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 1e9 + 7;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/
void solve(ll n, ll m)
{
    vector<ll> a(n);
    for (int i = 0; i < n; i++)
        cin >> a[i];

    ll full_weeks = m / 7;
    ll weekend_days = full_weeks * 2;

    ll rem = m % 7;
    if (rem >= 6)
        weekend_days++;

    sort(a.begin(), a.end());
    a.erase(unique(a.begin(), a.end()), a.end());

    ll extra_non_working = 0;

    for (ll x : a)
    {
        if (x > m)
            break;
        ll d = x % 7;
        if (d == 6 || d == 0)
            continue;
        extra_non_working++;
    }

    cout << m - weekend_days - extra_non_working <<endl;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    while (true)
    {
        ll n, m;
        cin >> n >> m;
        if (!cin || (n == 0 && m == 0))
            break;
        solve(n, m);
    }
    return 0;
}
