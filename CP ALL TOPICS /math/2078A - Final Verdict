 void solve(){
int a,x,s=0;
cin>>a>>x;
int ar[a];
for(int i=0;i<a;i++){
    cin>>ar[i];s+=ar[i];
}
float  c=((s*1.0)/a);
cout<<((c==x)?"YES":"NO");

    cout<<endl;
 }
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    int t;cin>>t;
    while(t--)
    solve();
}
