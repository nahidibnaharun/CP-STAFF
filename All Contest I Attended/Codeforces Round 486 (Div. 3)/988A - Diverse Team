void solve(){
    int n,k ; cin >> n >> k;
    set<int> el; 
    vector<int>ans;

    for(int i=0 ;i<n;i++){
        int x; cin >>x; 
        if(!el.count(x)){
            ans.push_back(i); el.insert(x);
        }
    }

    if(int(ans.size())<k)cout<<"NO";
    else{
        cout<<"YES"<<endl;

        for(int i=0 ;i<k;i++)cout<<ans[i]+1<<" ";
        
    }

}

int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

