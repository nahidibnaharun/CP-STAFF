TASK: practice 
*/

 void solve(){
   int a,b,n;cin>>a>>b>>n;
   int ans=0;
   while(a<=n && b<=n){
    if(a<b)a+=b;
    else b+=a;
    ans++;
   }
   cout<<ans;
    cout<<endl;
 }
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    int t;cin>>t;
    while(t--)
    solve();

       
}


