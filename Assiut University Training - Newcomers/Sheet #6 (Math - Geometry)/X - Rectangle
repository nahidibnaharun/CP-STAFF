void solve(){
double x1,y1,x2,y2,x3,y3,x4,y4;
    cin>>x1>>y1>>x2>>y2>>x3>>y3>>x4>>y4;
    double mx=min({x1,x2,x3,x4});
    double mxx=max({x1,x2,x3,x4});
    double my=min({y1,y2,y3,y4});
    double mxy=max({y1,y2,y3,y4});

    int t;
    cin>>t;
    while(t--){
        double x,y;
        cin>>x>>y;
        if(x>=mx&&x<=mxx&&y>=my&&y<=mxy) cout<<"YES";
        else cout<<"NO";
        cout<<endl;
    }



}

