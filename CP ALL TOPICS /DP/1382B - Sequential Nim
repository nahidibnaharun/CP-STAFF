void solve() {

 ll n, one = 0, no = 0, x;
        cin >> n;
 
        while(n--) {
            cin >> x;
            if(x==1 && !no) one++;
            else no = 1;
        }
 
        if(!no) one--;
 
        if(one%2) cout << "Second";
        else cout << "First";
}

int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

  ll Test_Case;
  cin >> Test_Case;
  while (Test_Case--) {
    solve();
    cout << endl;
  }

  return 0;
}
