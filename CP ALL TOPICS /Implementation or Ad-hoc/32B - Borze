void solve(){
string s;
cin>>s;
for(int i=0;i<s.size();i++){
    if(s[i]=='.')cout<<0;
    else if(s[i]=='-'&& s[i+1]=='.'){
        cout<<1;i++;
    }
    else if(s[i]=='-'&& s[i+1]=='-'){
        cout<<2;i++;
    }
}

}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
    // int t;cin>>t;
    // while(t--)
    solve();
   return 0;   
}


