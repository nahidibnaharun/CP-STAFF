int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    int n;cin>>n;
    int ans=0,sum=(n*(n+1)*1.0)/2;
    while(n--){
        int x;
        cin>>x;ans+=x;
    }
    if(ans==sum)cout<<"Yes";else cout<<"No";
   return 0;
    
}
