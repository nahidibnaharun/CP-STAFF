void solve(){
    int n,ans=0;cin>>n;
    vector<string>s(n);for(auto &x:s)cin>>x;
    int chk[n]={0};
    for(int i=0;i<n-1;i++){
        if(chk[i]==0){
            for(int j=i+1;j<n;j++){
                if(chk[j]==0){
                    string nw=s[i]+s[j],anw=s[j]+s[i];
                    if(stoll(nw)%3==0 || stoll(anw)%3==0){
                        chk[i]=chk[j]=-1;
                        ans++;
                        break;
                    }
                }
            }
        }
    }
    cout<<ans;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }    
return 0;
}
