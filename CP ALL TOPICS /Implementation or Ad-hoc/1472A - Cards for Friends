#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const int M=200004;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/ 

void solve(){
    int w,h,n;cin>>w>>h>>n;
    int ans=1;
    while(w%2==0){w/=2;ans*=2;}
    while(h%2==0){h/=2;ans*=2;}
    cout<<(ans>=n?"YES":"NO");
    cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
   int t;cin>>t;
    while(t--)
        solve();
}

       
