void solve(){
    ll n ; cin >> n; set<ll>s; 
    for(ll i=0 ;i<n;i++){
        ll in ; cin >> in ; s.insert(in);
    }
    if(n<4 || s.size() >2)cout<<n;
    else cout<<(n/2)+1;
}
 
 
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    
    


    ll Test_Case; cin >> Test_Case;
    while(Test_Case--){
        solve();
        cout<<endl;
    }
    return 0;
}

