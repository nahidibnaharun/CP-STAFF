void solve(){
   int n;cin>>n;
   int e=0,o=0,p=0,ne=0;
   while(n--){
    int x;cin>>x;
    if(x%2==0)e++;
    else o++;
    if(x>0)p++;
    else if(x<0)ne++;
   }
   cout<<"Even: "<<e<<endl;
   cout<<"Odd: "<<o<<endl;
   cout<<"Positive: "<<p<<endl;
   cout<<"Negative: "<<ne<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
