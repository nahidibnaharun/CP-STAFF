void solve()
{
    int n;
    cin >> n;
    string s = string(n, '0');
    s[n / 2] = '1';
    cout << s;
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;

    for (int i = 1; i <= t; i++)
    {
        solve();
        cout << endl;
    }

    return 0;
}
