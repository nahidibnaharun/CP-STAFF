#include <bits/stdc++.h>
#include <unordered_map>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 1006903069;
const int MAX = 1e7 + 10;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

void solve() {
  
  int n;
    cin >> n;

    vector<ll> a(n);
    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }

    sort(a.begin(), a.end());

    vector<ll> pre(n + 1, 0), suf(n + 1, 0);

    for (int i = 0; i < n; i++) {
        pre[i + 1] = pre[i] + a[i];
    }

    for (int i = n - 1; i >= 0; i--) {
        suf[n - i] = suf[n - i - 1] + a[i];
    }

    bool ok = false;

    for (int k = 1; k <= n; k++) {
        if (2 * k + 1 <= n) {
            ll b = pre[k + 1];
            ll r = suf[k];
            if (b < r) {
                ok = true;
                break;
            }
        }
    }

    if (ok) cout << "YES" ;
    else cout << "NO" ;

}

int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--){
  solve();
      cout<<endl;
    }

  return 0;
}
