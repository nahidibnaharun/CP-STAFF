void solve() {
    
ll n,cnt=0; cin >> n; 
for(int i=1 ; i<=n ;i++){
    if(n%i==0)cnt++;
    else break;
}
cout<<cnt;

}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    
    


    ll Test_Case;
    cin >> Test_Case;
    while(Test_Case--){
        solve();
        cout<<endl;
    }
    return 0;
}
