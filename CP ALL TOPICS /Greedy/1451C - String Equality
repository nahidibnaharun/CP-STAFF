#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 10000;
const ll MAXN = 2e5 + 5;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

void solve()
{
    int n, k;
    string a, b;
    cin >> n >> k >> a >> b;

    array<int, 27> d{}, add{};

    for (auto &c : a)
        d[c - 'a']++;

    for (auto &c : b)
        add[c - 'a']++;

    bool chk = false;

    for (int i = 0; i < 26; i++)
    {
        if (d[i] < add[i] || ((d[i] -= add[i]) % k))
            chk = true;

        d[i + 1] += d[i];
    }

    if (chk)
        cout << "No";
    else
        cout << "Yes";
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
