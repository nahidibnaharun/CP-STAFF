#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 50000 + 5;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

vector<int> divs[MAX];
int a[MAX][26];

void solve()
{
    int n, k;
    cin >> n >> k;

    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < 26; j++)
        {
            a[i][j] = 0;
        }
    }

    vector<string> s(k);

    for (int i = 0; i < k; i++)
    {
        cin >> s[i];
        for (int j = 0; j < n; j++)
        {
            a[j][s[i][j] - 'a'] = 1;
        }
    }

    for (auto d : divs[n])
    {
        string res = "";
        bool ok = true;

        for (int i = 0; i < d; i++)
        {
            vector<int> v(26, 1);

            int j = i;
            while (j < n)
            {
                for (int c = 0; c < 26; c++)
                {
                    if (a[j][c] == 0)
                        v[c] = 0;
                }
                j += d;
            }

            bool found = false;
            for (int c = 0; c < 26; c++)
            {
                if (v[c])
                {
                    res += char(c + 'a');
                    found = true;
                    break;
                }
            }

            if (!found)
            {
                ok = false;
                break;
            }
        }

        if (ok)
        {
            string ans = "";
            for (int i = 1; i <= n / d; i++)
                ans += res;

            cout << ans;
            return;
        }
    }
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    for (int i = 1; i < MAX; i++)
    {
        for (int j = i; j < MAX; j += i)
        {
            divs[j].push_back(i);
        }
    }

    int t = 1;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }

    return 0;
}
