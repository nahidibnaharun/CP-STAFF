void solve(){
 string s; cin >> s;
    string target = "hello";
    int idx = 0;
    for (char c : s) {
        if (c == target[idx]) idx++;
        if (idx == target.size()) break;
    }
    if (idx == target.size()) cout << "YES";
    else cout << "NO";
}
