#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const ll INF = 1000000007;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
ll n,m,k;cin>>n>>m>>k;
if(k>n+m-2){cout<<-1;return;}
ll ans=0,rmk=k;
if(rmk<n){
    ll gp=rmk+1,mepg=n/gp,lgp=n%gp,sgp=gp-lgp,cmc=min(mepg+1,mepg);
    cmc=n/gp;
    ans=max(ans,cmc*m);
}
else{
    rmk-=(n-1);
    ll gp=rmk+1,mepg=m/gp;ans=max(ans,mepg);
}
swap(n,m);rmk=k;
if(rmk<n){
    ll gp=rmk+1,mepg=n/gp;ans=max(ans,mepg*m);
}
else{
    rmk-=(n-1);
    ll gp=rmk+1,mepg=m/gp;ans=max(ans,mepg);
}
cout<<ans;

}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
        // cout<<endl;
    // }
return 0;
}
