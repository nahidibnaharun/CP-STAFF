#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const int INF=500;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
    int n;cin>>n;vector<int>a(n);for(auto &x:a)cin>>x;
    if(n==0){cout<<0;return;}
    vector<int>sf_mex(n+1,0);
    vector<bool>sn_suf(n+2,false);
    int cmex=0;
    for(int i=n-1;i>=0;--i){
        sn_suf[a[i]]=true;
        while(sn_suf[cmex])cmex++;
        sf_mex[i]=cmex;
    }
    sf_mex[n]=0;
    vector<int>b;
    int p=0;
    while(p<n){
        int tmex=sf_mex[p];
        vector<bool>sn_pf(n+2,false);
        int pmex=0,j=p;
        while(j<n){
            sn_pf[a[j]]=true;
            while(sn_pf[pmex])pmex++;
            if(pmex==tmex){
                b.push_back(tmex);p=j+1;break;
            }j++;
        }
    }
    cout<<b.size()<<endl;
    for(auto x:b)cout<<x<<" ";

}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
    solve();
    cout<<endl;
    }
}
