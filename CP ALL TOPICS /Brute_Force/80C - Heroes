#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const ll INF = (1LL<<60);
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
map<string,int> ar;
    ar["Anka"] = 0;
    ar["Chapay"] = 1;
    ar["Cleo"] = 2;
    ar["Troll"] = 3;
    ar["Dracul"] = 4;
    ar["Snowy"] = 5;
    ar["Hexadecimal"] = 6;
    int n;
    while(cin>>n){
        int g[7][7];
        memset(g,0,sizeof(g));
        for(int i=0;i<n;++i){
            string p,s,q;cin>>p>>s>>q;
            g[ar[p]][ar[q]]=1;
        }
        ll vl[3],bdf=INF;cin>>vl[0]>>vl[1]>>vl[2];
        int bk=0,tl=1;
        for(int i=0;i<7;++i)tl*=3;
        int tm[7];
        for(int mk=0;mk<tl;++mk){
            int t=mk,cnt[3]={0};
            for(int i=0;i<7;++i){
                tm[i]=t%3;++cnt[tm[i]];t/=3;
            }
            if(cnt[0]==0 || cnt[1]==0 || cnt[2]==0)continue;
            ll per[3];
            for(int k=0;k<3;++k)per[k]=vl[k]/cnt[k];
            ll mn=min(per[0],min(per[1],per[2])),mx=max(per[0],max(per[1],per[2])),df=mx-mn;
            int lk=0;
            for(int i=0;i<7;++i)
                for(int j=0;j<7;++j)
                    if(i!=j && tm[i]==tm[j] && g[i][j])++lk;
            
            if(df<bdf){bdf=df;bk=lk;}
            else if(df==bdf){
                if(lk>bk)bk=lk;
            }
        }
cout<<bdf<<" "<<bk;
    }
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
        // cout<<endl;
    // }
return 0;
}
