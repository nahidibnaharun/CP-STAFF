#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const int M=200004;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/ 
void solve(){
  ull a,b,n;
  cin>>a>>b>>n;
  a=(a%n==0)?a/n:(a/n+1);
  b=(b%n==0)?b/n:(b/n+1);
  cout<<a*b;
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   // int t;cin>>t;
   // while(t--)
    solve();
} 
