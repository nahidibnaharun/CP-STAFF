#include <bits/stdc++.h>
using namespace std;
using ll = long long;

void solve(){
int n;cin>>n;
    vector<int>d={500,100,50,10,5,1},ct(d.size(),0);
    for (int i=0;i<d.size();++i){
        ct[i]=n/d[i];n-= ct[i]*d[i];
    }
    bool f=1;
    for(int i=d.size()-1;i>=0;--i){
        for (int j=0;j<ct[i];++j) {
            if (!f)cout<<' ';
            cout<<d[i];
            f=0;
        }
    }
}
int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    // int t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
    return 0;
}
