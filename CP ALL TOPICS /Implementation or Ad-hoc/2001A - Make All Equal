#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const ll M=1e9+1;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
int n;cin>>n;
vector<int>ar(n);
for(auto &x:ar)cin>>x;
vector<int>fr(n+1);
for(int x:ar)fr[x]++;
cout<<n-(*max_element(fr.begin(),fr.end()))<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--)
    solve();
} 
