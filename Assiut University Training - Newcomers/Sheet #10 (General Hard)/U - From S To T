#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int MOD = 676767677LL;
const int MAX = 1e7 + 5;
/*
//      _   _  __  _   _
_____
_____
//     / | / / / _ \ \ \__\ \\__  _\\  _  \ 
//    /  |/ / / /_\ \ \  _ \  \ \   \ \ \  \ 
//   / /|  / / __  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

void solve(){
 
string s,t,p;
cin >>s>>t>>p;

int i=0;
for(int j=0;j<t.size() && i<s.size();j++){
    if(s[i]==t[j])i++;
}
if(i!=s.size()){
    cout<<"NO";
    return;
}

int cnt[26]={0};
for(auto x:t)cnt[x-'a']++;
for(auto x:s)cnt[x-'a']--;
for(auto x:p)cnt[x-'a']--;

for(int i=0;i<26;i++){
    if(cnt[i]>0){
        cout<<"NO";return;
    }
}
cout<<"YES";


}

    

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   ll t;cin>>t;
   while(t--){
        solve();
        cout<<endl;
   }
return 0;
}
    
