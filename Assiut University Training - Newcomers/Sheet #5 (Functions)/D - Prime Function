bool isprime(ll n){
    if(n<2)return false;
    if(n==2)return true;
    if(n%2==0)return false;
    for(ll i=3;i*i<=n;i+=2){
        if(n%i==0)return false;
    }
    return true;
}
void solve(){
ll n;cin>>n;
cout<<(isprime(n)?"YES":"NO");
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
