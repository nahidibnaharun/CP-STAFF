LANG: C++
TASK: practice 
*/

void solve() { 
    auto check = [](ll mid, int n, const vector<ll>& a) {
        long double sum = 0.0;
        for (ll val : a) sum += logl(val) - logl(mid);
        return sum < 0;
    };

    int n;
    cin >> n;
    vector<ll> a(n);
    for (auto &x : a) cin >> x;

    ll left = 1, right = 2e10, ans = right;
    while (left <= right) {
        ll mid = left + (right - left) / 2;
        if (mid == 0) {
            left = 1;
            continue;
        }
        if (check(mid, n, a)) {
            ans = mid;
            right = mid - 1;
        } else {
            left = mid + 1;
        }
    }
    cout << ans;
    


}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // ll t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
return 0;
}
