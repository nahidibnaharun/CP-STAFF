void solve(){
    vector<int>a(4);
    for(int i=0;i<4;i++)cin>>a[i];
    ll ky=a[0],v=1;
    if(a[ky]==0)v=0;
    else ky=a[ky];
    if(a[ky]==0)v=0;
    else ky=a[ky];
    if(v)cout<<"YES";
    else cout<<"NO";
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--){
  solve();
  cout<<endl;
  }
}
