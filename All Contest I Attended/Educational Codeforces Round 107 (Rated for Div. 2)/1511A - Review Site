void solve() {
    int n;
    cin >> n;
    vector<int> rv(n);
    for (int i = 0; i < n; i++) {
        cin >> rv[i];
    }
    
    int t1 = 0, t2 = 0, t3 = 0;
    for (int r : rv) {
        if (r == 1) t1++;
        else if (r == 2) t2++;
        else if (r == 3) t3++;
    }

    int upv = t1;
    
     upv += t3;

    cout << upv;
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
