void solve(){
int s;cin>>s;
string res;
for(int i=9;i>=1;i--){
    if(s>=i){res=char('0'+i)+res;s-=i;}
}
cout<<res;
cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--)
    solve();
}
