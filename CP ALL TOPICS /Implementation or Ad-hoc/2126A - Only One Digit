void solve(){
    int x;cin>>x;string s=to_string(x);
    char n='9';
    for(char c:s)n=min(n,c);
    cout<<n-'0';
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--){
  solve();
  cout<<endl;
  }
}
