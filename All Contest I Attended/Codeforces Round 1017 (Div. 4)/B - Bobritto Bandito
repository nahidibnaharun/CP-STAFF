int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
    int t;
    cin>>t;
    while(t--){
        int n,m,l,r;
        cin>>n>>m>>l>>r;
        int lm=-l,rm=r,x=max(0,m-rm);
        x=min(x,lm);
        cout<<-x<<" "<<m-x<<endl;
    }
    
    return 0;   
}
