
#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 10000;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

ll a[MAX];
vector<ll> v[MAX];

void solve()
{
    ll n, q;
    cin >> n >> q;

    for (ll i = 1; i <= n; i++)
    {
        cin >> a[i];
        v[a[i]].push_back(i);
    }

    mt19937 rng(chrono::steady_clock::now().time_since_epoch().count());

    while (q--)
    {
        ll l, r;
        cin >> l >> r;

        ll ans = 1;

        for (ll i = 0; i < 40; i++)
        {
            ll idx = uniform_int_distribution<ll>(l, r)(rng);
            ll c = a[idx];

            ll f = upper_bound(v[c].begin(), v[c].end(), r) -
                   lower_bound(v[c].begin(), v[c].end(), l);

            ans = max(ans, 2 * f - (r - l + 1));
        }

        cout << ans << endl;
    }
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t = 1;
    // cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
