#include<bits/stdc++.h>
using namespace std;
using ll = long long;
/*
ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
  int a,b;
  cin>>a>>b;
  b++;
  int d=1;
  for(int i=a;i>0;i--){
    if(i<=b){
      cout<<d<<" ";
      d++;
    }
    else cout<<i<<" ";
  }cout<<endl;
  return;
}

int main(){
  int t;
  cin>>t;
  while(t--){
    solve();
  }
  return 0;
}
