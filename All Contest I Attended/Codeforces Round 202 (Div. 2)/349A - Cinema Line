void solve(){
int n;cin>>n;
int n25=0,n50=0;
int ar[n];
for(auto &x:ar)cin>>x;

for(auto x:ar){
    if(x==25)n25++;
    else if(x==50){
        n50++;
        if(n25>0)  n25--;
        else {cout<<"NO";return;}
    }
    else if(x==100){
        if(n25>0 && n50>0){n25--;n50--;}
        else if(n25>2)n25-=3;
        else {cout<<"NO";return;}
    }
}
cout<<"YES";
}



int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
        // cout<<endl;
    // }    
return 0;
}
