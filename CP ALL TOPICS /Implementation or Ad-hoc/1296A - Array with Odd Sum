#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const int MAXN = 200000 + 5;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/      
void solve(){
ll n,ev=0,od=0,x;
cin>>n;
while(n--){
   cin>>x;
   if(x%2==0)ev++;
   else od++;
}
if(od==0 || (ev==0 && od%2==0))cout<<"NO";
else cout<<"YES";
cout<<endl;
}

int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    ll t;cin>>t;
    while(t--)
    solve();    
}
