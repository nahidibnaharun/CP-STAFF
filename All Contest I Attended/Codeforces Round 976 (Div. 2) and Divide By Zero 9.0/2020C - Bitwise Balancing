
#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 200007;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

void solve()
{

    ll a = 0, b, c, d;
    cin >> b >> c >> d;

    ll mask = 1;
    bool ok = true;

    for (ll i = 0; i < 62; i++)
    {
        ll bit_b = (b & mask) ? 1 : 0;
        ll bit_c = (c & mask) ? 1 : 0;
        ll bit_d = (d & mask) ? 1 : 0;

        if ((bit_b && !bit_c && !bit_d) || (!bit_b && bit_c && bit_d))
        {
            ok = false;
            break;
        }

        if (bit_b && bit_c)
        {
            a += (1LL - bit_d) * mask;
        }
        else
        {
            a += bit_d * mask;
        }

        mask <<= 1;
    }

    if (ok)
        cout << a << endl;
    else
        cout << -1 << endl;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        // cout << endl;
    }
    return 0;
}
