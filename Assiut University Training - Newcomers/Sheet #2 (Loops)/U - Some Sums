ll sod(int n){
    ll ans=0;
    while(n!=0){
        ans+=n%10;
        n=n/10;
    }
    return ans;
}
void solve(){
  int n,a,b;cin>>n>>a>>b;
  ll ans=0;
  for(int i=1;i<=n;i++){
    ll soi=sod(i);
    if(min(a,b)<=soi && max(a,b)>=soi)ans+=i;
  }
  cout<<ans;

}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
return 0;
}
