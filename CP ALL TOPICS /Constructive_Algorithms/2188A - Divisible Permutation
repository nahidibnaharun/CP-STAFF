
#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 1e5 + 7;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/
void solve()
{
    int n;
    cin >> n;

    int x = 1;
    vector<int> ans;
    ans.push_back(1);

    for (int i = n - 1, j = 1; i >= 1; i--, j++)
    {
        if (j % 2)
            x = x + i;
        else
            x = x - i;

        ans.push_back(x);
    }

    reverse(ans.begin(), ans.end());

    for (int v : ans)
        cout << v << " ";
    cout << endl;
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
    }
    return 0;
}
