void solve(){
int n;cin>>n;
vector<vector<int>>ar(n,vector<int>(n,-1));
int a=0,b=0;
for(int i=0;i<n;i++){
ar[i][a]=ar[i][b]=1;
b++;
}
a=1;
for(int i=2;i<n;i++){
    for(int j=1;j<=a;j++){
        ar[i][j]=ar[i-1][j-1]+ar[i-1][j];
    }a++;
}
for(int i=0;i<n;i++){
    for(int j=0;j<n;j++){
        if(ar[i][j]!=-1)
        cout<<ar[i][j]<<" ";
    }
    cout<<endl;
}
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
return 0;
}
