void solve() {
int n,m;
cin>>n>>m;
bool t[n]={0};
while(m--){
int l;
char g;
cin>>l;
cin>>g;
l--;
if(t[l]==0 && g=='M'){cout<<"Yes";
t[l]=1;}
else  {cout<<"No";
}
cout<<endl;
}
return ;
}

int main() {
 
   solve();
 

  return 0;
}
