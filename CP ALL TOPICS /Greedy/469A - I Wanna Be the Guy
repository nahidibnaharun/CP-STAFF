#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n,a,b,sum=0;
    cin>>n>>a;
    set<int> s;
    while(a--){
        int t;
        cin>>t;
        s.insert(t);
    }
    cin>>b;
    while(b--){int t;
    cin>>t;
        s.insert(t);
    }
     for (int num : s) {
        sum += num;
    }
    if((n*(n+1))/2 -sum == 0) cout<<"I become the guy.";
      else cout<<"Oh, my keyboard!";
    
    return 0;
}
