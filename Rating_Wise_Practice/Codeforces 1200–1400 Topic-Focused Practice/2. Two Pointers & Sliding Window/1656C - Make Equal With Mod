#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int MOD = 676767677LL;
const int MAX = 1e7 + 5;
/*
//      _   _  __  _   _
_____
_____
//     / | / / / _ \ \ \__\ \\__  _\\  _  \ 
//    /  |/ / / /_\ \ \  _ \  \ \   \ \ \  \ 
//   / /|  / / __  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

void solve(){
    int n; cin >>n;
    bool one =false;
    vector<ll>a(n);
    for(auto &x:a){cin >>x;
    if(x==1)one=true;}
    bool chk=true;
    for(int i=0;i<n-1;i++){
        if(a[i]!=a[i+1]){
            
            chk=false;break;
        }
    }
    if(chk){
        cout<<"Yes";return;
    }
    if(one==false)cout<<"Yes";
    else{
        sort(a.begin(),a.end());
        bool chk=false;
        for(int i=0;i<n-1;i++){
            if(a[i]+1==a[i+1]){
                chk=true;break;
            }
        }
        if(chk)cout<<"No";
        else cout<<"Yes";

    }
    
}

    

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll t;cin>>t;
   while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
    
