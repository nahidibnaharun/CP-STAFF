vector<vector<int>>ar;
void rowswap(int x, int y,int n){
    for(int i=0;i<n;i++)
    swap(ar[x][i],ar[y][i]);
}
void colswap(int x, int y,int n){
    for(int i=0;i<n;i++)
    swap(ar[i][x],ar[i][y]);
}
void solve(){
int n,x,y;
cin>>n>>x>>y;
ar.resize(n,vector<int>(n));
for(int i=0;i<n;i++)
    for(int j=0;j<n;j++)
        cin>>ar[i][j];

rowswap(x-1,y-1,n);
colswap(x-1,y-1,n);
for(int i=0;i<n;i++){
    for(int j=0;j<n;j++)cout<<ar[i][j]<<" ";
    cout<<endl;
}
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
