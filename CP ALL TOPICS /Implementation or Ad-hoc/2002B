#include <bits/stdc++.h>

using namespace std;

using ll = long long;

/*

ID: mdnahidibnaharun

LANG: C++

TASK: practice

*/

void solve() {
	int n;
	cin>>n;
	vector<int> a(n),b(n);
	for(int i=0;i<n;i++)cin>>a[i];
	for(int i=0;i<n;i++)cin>>b[i];
vector<int> c=b;
reverse(c.begin(),c.end());
if(a==b||a==c)cout<<"Bob";
else cout<<"Alice";
cout<<endl;
return ;
}

int main() {
 int n;
 cin>>n;
 while(n--)
   solve();
 

  return 0;
}
