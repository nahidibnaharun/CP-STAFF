#include <bits/stdc++.h>

using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 1e9 + 7;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/
void solve()
{
    int n, m;
    cin >> n >> m;

    vector<string> grid1(n), grid2(n);
    for (int i = 0; i < n; i++)
        cin >> grid1[i];
    for (int i = 0; i < n; i++)
        cin >> grid2[i];

    int sr1[m]{}, sr2[m]{}, sc1[n]{}, sc2[n]{};

    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < m; j++)
        {
            sr1[j] += (grid1[i][j] - '0');
            sr1[j] %= 3;
            sr2[j] += (grid2[i][j] - '0');
            sr2[j] %= 3;
            sc1[i] += (grid1[i][j] - '0');
            sc1[i] %= 3;
            sc2[i] += (grid2[i][j] - '0');
            sc2[i] %= 3;
        }
    }

    bool ok = true;
    for (int i = 0; i < n; i++)
        ok &= (sc1[i] == sc2[i]);
    for (int i = 0; i < m; i++)
        ok &= (sr1[i] == sr2[i]);

    if (ok)
        cout << "YES";
    else
        cout << "NO";
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    ll Test_Case;
    cin >> Test_Case;
    while (Test_Case--)
    {
        solve();
        cout << endl;
    }

    return 0;
}
