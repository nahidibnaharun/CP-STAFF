void solve(){
    
    int n;
    cin >> n;
    vector<int> a(n);
    for (int& x : a) cin >> x;
    cout << n - count(a.begin(), a.end(), *min_element(a.begin(), a.end()));

}






int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

  ll Test_Case; cin >> Test_Case;
  while (Test_Case--) {
  solve();
  cout<<endl;
  }

  return 0;
}
