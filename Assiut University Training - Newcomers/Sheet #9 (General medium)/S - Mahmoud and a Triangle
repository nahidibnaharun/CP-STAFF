void solve() {
    
    int n;  
    cin >> n;
    ll ar[n];
    for(auto &x: ar)cin>>x;
    sort(ar , ar+n);

    for(int i=0;i<n-2;i++){
        if(ar[i] + ar[i+1] >ar[i+2]){
            cout<<"YES";return;
        }
    }
    cout<<"NO";

}


