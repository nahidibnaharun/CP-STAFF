void solve(){
    string s;getline(cin,s);
    int h[26]={0},hu[26]={0};
    for(auto x:s){
        if(x-' '!=0){
            if(x-'a'>-1)h[x-'a']++;
            else hu[x-'A']++;
        }
    }
    string l;getline(cin,l);
    for(auto x:l){
        if(x!=' '){
            int id=x-'a';
            if(id>-1){
                if(h[id]>0)h[id]--;
                else{cout<<"NO";return;}
            }
            else{
                if(hu[x-'A']>0)hu[x-'A']--;
                else {cout<<"NO";return;}
            }
        }
    }cout<<"YES";
}
 
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }    
return 0;
}
