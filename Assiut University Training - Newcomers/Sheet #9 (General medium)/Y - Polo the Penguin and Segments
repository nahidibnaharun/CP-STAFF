void solve() {
   int n,k;
   cin >> n >> k;
   n = 2*n+1;
   vector<int> a(n);
   for(auto &x: a) cin >> x;
   for(int i=1;i< n-1 ;i++){
    if(k>0){
        if(a[i-1] < a[i]-1  && a[i] - 1 > a[i+1]){
            a[i]--;
            k--;
        }
    }
    else break;
   }
   for(auto &x : a)cout<< x<< " ";
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // ll t;cin>>t;
    // while(t--){
        solve();
        // cout<<endl;
    // }
return 0;
}
