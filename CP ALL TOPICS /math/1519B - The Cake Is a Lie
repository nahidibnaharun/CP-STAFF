void solve(){
int n,m,k;cin>>n>>m>>k;
if((n*m)-1==k)cout<<"YES";
else cout<<"NO";
cout<<endl;
}
        
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--)
    solve();
} 
