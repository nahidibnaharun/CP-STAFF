void solve(){
    int n;cin>>n;
    int no=0;
    for(int i=0;i<n;++i){
        int x;cin>>x;
        if(x!=1){
            no+=(x-1);
        }

    }
    if(no%2==0)cout<<"maomao90";
    else cout<<"errorgorn";
 }
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
