void solve() {
string s;
cin>>s;
int z=s.size();
if(s[0]=='1' && s[1]=='0' && z>=3){
if(z>3){
  if(s[2]!='0')cout<<"YES";
  else cout<<"NO";
}
else{
  if(s[2]!='0' && s[2]!='1')cout<<"YES";
  else cout<<"NO";
}
}
else cout<<"NO";
cout<<endl;
return ;
}

int main() {
 int n;
 cin>>n;
 while(n--)
   solve();
 

  return 0;
}
