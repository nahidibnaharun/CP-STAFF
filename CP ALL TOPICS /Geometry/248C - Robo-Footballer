#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 1e7 + 10;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

void solve()
{
    ld y1, y2, yw, xb, yb, r;
    cin >> y1 >> y2 >> yw >> xb >> yb >> r;

    ld ye = yw - r;
    ld yb_m = 2 * ye - yb;
    ld yt = y1 + r;
    ld xw = xb * (ye - yt) / (yb_m - yt);

    ld A = yb_m - yt;
    ld B = -xb;
    ld C = xb * yt;

    ld dnum2 = (B * y2 + C) * (B * y2 + C);
    ld dden2 = A * A + B * B;

    if (dnum2 > r * r * dden2 + 1e-15)
        cout << fixed << setprecision(10) << (double)xw;
    else
        cout << -1;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    // cin >> t;
    t = 1;
    while (t--)
    {
        solve();
        // cout << endl;
    }
    return 0;
}
