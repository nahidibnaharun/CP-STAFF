#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 998244353;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

void solve()
{
    int n, l, r;
    cin >> n >> l >> r;
    vector<ll> pref(n + 1, 0), a(n + 1, 0);
    ll cur = 1;
    for (int i = 1; i <= n; i++)
    {
        if (i == r)
            pref[i] = pref[l - 1];
        else
        {
            if (cur == pref[l - 1])
                cur++;
            pref[i] = cur++;
        }
    }

    for (int i = 1; i <= n; i++)
    {
        a[i] = pref[i] ^ pref[i - 1];
    }
    for (int i = 1; i <= n; i++)
    {
        cout << a[i] << " ";
    }
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    ll Test_Case;
    cin >> Test_Case;
    while (Test_Case--)
    {
        solve();
        cout << endl;
    }

    return 0;
}
