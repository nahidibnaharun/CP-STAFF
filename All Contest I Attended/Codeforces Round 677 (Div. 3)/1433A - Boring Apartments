void solve(){
string s;cin>>s;
int n=s[0]-'1'+1;
int size=s.size();
int ans=(n-1)*10;
for(int i=1;i<=size;i++)ans+=i;
cout<<ans;

cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
    int t;cin>>t;
    while(t--)
    solve(); 
}

