#include <algorithm>
#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const ll INF = 1e17;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
ll fun(ll n){
    ll ans=1;
    for(int i=1;i<=n;i++)ans= ans*10;
    return ans;
}
void run(ll x,ll n){
    if(n<=0)return;
    ll div=fun(n-1);
    cout<<x/div<<" ";
    run(x%div,n-1);

}
void solve(){
ll x;cin>>x;
string s=to_string(x);
ll n=s.size();
run(x,n);

 }

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
