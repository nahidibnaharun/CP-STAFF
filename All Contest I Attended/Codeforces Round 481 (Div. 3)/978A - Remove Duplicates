void solve(){
    int n; cin >> n; vector<int>a(n),ans; for(auto &x : a)cin >>x;
    bool chk=false;

    for(int i=0 ;i<n;i++){
        chk=false;
        for(int j=i+1;j<n;j++){
            if(a[i]==a[j]){chk=true;break;}
        }
        if(!chk)ans.push_back(a[i]);
    }
    cout<<ans.size()<<endl;
    for(auto x:ans)cout<<x<<" ";
}


int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

//   ll Test_Case; cin >> Test_Case;
//   while (Test_Case--) {
  solve();
//   cout<<endl;
//   }

  return 0;
}
