LANG: C++
TASK: practice 
*/ 

void solve(){
   int n;cin>>n;
   if(n%2){
    for(int i=n;i>0;i--){
        if(i==(n/2)+1)cout<<1;
        else if(i==1)cout<<n/2+1;
        else cout<<i;
        cout<<" ";
    }
   }
   else{
    for(int i=n;i>0;i--)cout<<i<<" ";
   }
cout<<endl;

}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   int t;cin>>t;
    while(t--)
        solve();
}

