const long double pi = 3.1416L;
long double s;
const long double chk = 1e-12L;
void solve(){
while(cin>>s){
    if(s<=chk){
        cout<<"BAD LUCK"<<endl;continue;
    }
    long double ar=s*s*(1.0L-pi/4.0L);
    if(ar<=chk)cout<<"BAD LUCK"<<endl;
    else cout<<fixed<<setprecision(2)<<(double)ar<<endl;

}


}
  
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
    solve();
    // cout<<endl;
    // }
}
