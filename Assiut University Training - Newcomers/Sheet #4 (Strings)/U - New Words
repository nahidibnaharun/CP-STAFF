void solve(){
string s;cin>>s;
transform(s.begin(),s.end(),s.begin(), ::tolower);
int ar[26]={0};
for(auto x:s)ar[x-'a']++;
cout<<min({ar['e'-'a'],ar['g'-'a'],ar['y'-'a'],ar['p'-'a'],ar['t'-'a']});

}
