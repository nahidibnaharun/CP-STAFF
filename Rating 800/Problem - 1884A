#include<bits/stdc++.h>
using namespace std;
using ll = long long;
/*
ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
int sa(int n){
	int s=0;
	while(n!=0){
		s+=n%10;
		n/=10;
	}
	return s;
}
void solve(){
	int n,k;
	cin>>n>>k;
for(int i=n;;i++){
	if(sa(i)%k==0){
		cout<<i<<endl;
		break;
	}
}
return;
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int t;
	cin>>t;
	while(t--){
	solve();
	
	}
return 0;
}
