#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 998244353;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

ll ans(ll x, ll y, ll n)
{
    x %= n;

    ll res = 1;
    while (y > 0)
    {
        if (y & 1)
            res = res * x % n;
        x = x * x % n;
        y >>= 1;
    }
    return res;
}

void solve()
{

    ll x, y, n;
    cin >> x >> y >> n;

    cout << ans(x, y, n);
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    ll Test_Case;
    cin >> Test_Case;
    while (Test_Case--)
    {
        solve();
        cout << endl;
    }

    return 0;
}
