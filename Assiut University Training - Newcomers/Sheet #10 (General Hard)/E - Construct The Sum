#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int MOD = (int)(1e9) + 7;
const int MAX = 1e7 + 5;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

void solve() { 
    ull s,n;
    cin >> s >> n;
    if( (s*(s+1))/2 < n){
        cout << -1;
        return;
    }
    if(s > n){
        cout << 1 <<" "<< n;
        return;
    }
    ull hn = n / s;
    
    if(n%s ==0){
        cout << hn ;
        while(hn--){
            cout<<" "<<s;
        }
    }
    else{
        cout << hn+1;
        while(hn--){
            cout << " "<<s;
        }
        cout <<" "<< n%s;
    }



}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
