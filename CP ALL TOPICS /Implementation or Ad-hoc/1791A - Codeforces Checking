void solve(){
int n;cin>>n;
string tm="codeforces";
unordered_set<char> cs(tm.begin(),tm.end());
while(n--){
char c;cin>>c;
if(cs.count(c)>0)cout<<"Yes"<<endl;
else cout<<"No"<<endl;
}
}
int main(){
