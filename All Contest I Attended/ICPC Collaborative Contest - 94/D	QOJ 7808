#include <bits/stdc++.h>
#define endl '\n'

using namespace std;
using ll = long long int;

bool isvowel(char c) {
	c = tolower(c);
	if (c == 'a') return 1;
	if (c == 'e') return 1;
	if (c == 'i') return 1;
	if (c == 'o') return 1;
	if (c == 'u') return 1;
	if (c == 'y') return 1;

	return 0;
}

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0); cout.tie(0);

	// int tt; cin >> tt;
	// while(tt--) {
	string s, t; cin >> s >> t;

	int flag = 1, i = 0, j = 0;
	int n = s.size();
	int m = t.size();
	while (i < n && flag && j < m) {
		if (j < m) {
			if (tolower(s[i]) == tolower(t[j])) {
				i++, j++;
			}
			else if (isvowel(t[j])) j++;
			else {
				flag = 0;
				break;
			}
		}
		else {
			break;
		}
	}

	while (j < m) {
		if (isvowel(t[j])) j++;
		else break;
	}

	if (flag && j == m && i == n) cout << "Same\n";
	else cout << "Different\n";
	// }

	return 0;
}
