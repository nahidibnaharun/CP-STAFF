const int nm=1e6+7;

void solve(){
    vector<bool>prime(nm,true);
    
    prime[1]=false;
    for(int i=2;i*i<=nm;i++){
    for(int j=i*2;j<nm;j+=i)prime[j]=false;
    }
 int n,x;
 cin>>n>>x;
 vector<int>a(n);
 for(auto &l: a) cin>>l;
 for(int i=0; i<n;i++){
     int o;
     cin>>o;
     if(o==2){
     cout<<0<<" ";
     }
     else{
     if(prime[a[i]]==false)cout<<-1<<" ";
     else{
    if(a[i]==2){
        cout<<0<<" ";
    }
    else
     cout<<5<<" ";
     }
     }
     
 }
}

    

