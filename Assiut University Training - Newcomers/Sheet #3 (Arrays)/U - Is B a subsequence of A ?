void solve(){
int n,m;cin>>n>>m;
vector<ll>a(n),b(m);
for(auto &x:a)cin>>x;
for(auto &x:b)cin>>x;
int i=0,ans=0;
for(int j=0;j<n;j++){
    if(b[i]==a[j]){
        i++;ans++;
    }
}
if(ans==m)cout<<"YES";
else cout<<"NO";

}
    
