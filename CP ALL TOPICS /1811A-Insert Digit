void solve(){
int n,d;cin>>n>>d;string s;cin>>s;
for(int i=0;i<n;i++){
    if(s[i]-'0'>=d)cout<<s[i];
    else{
        cout<<d;for(int j=i;j<n;j++)cout<<s[j];
        cout<<endl;return;
    }
}cout<<d;
cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--)
    solve();
}
