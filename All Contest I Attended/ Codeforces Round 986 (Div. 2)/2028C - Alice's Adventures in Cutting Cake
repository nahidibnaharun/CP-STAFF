
#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 1e5 + 7;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

void solve()
{
    int n, m;
    cin >> n >> m;

    ll v;
    cin >> v;

    vector<ll> a(n);
    for (int i = 0; i < n; i++)
        cin >> a[i];

    vector<ll> prefix_sum(n + 1, 0);
    for (int i = 0; i < n; i++)
        prefix_sum[i + 1] = prefix_sum[i] + a[i];

    auto query = [&](int l, int r)
    {
        return prefix_sum[r] - prefix_sum[l];
    };

    auto compute_pfx = [&]() -> vector<int>
    {
        vector<int> pfx(n + 1, 0);
        int end = 0;
        ll sum = 0;

        for (int start = 0; start < n; start++)
        {
            while (end < n && sum < v)
            {
                sum += a[end];
                end++;
                pfx[end] = max(pfx[end], pfx[end - 1]);
            }

            if (sum >= v)
                pfx[end] = max(pfx[end], 1 + pfx[start]);

            sum -= a[start];
        }

        for (int i = 1; i <= n; i++)
            pfx[i] = max(pfx[i], pfx[i - 1]);

        return pfx;
    };

    vector<int> pfx = compute_pfx();

    reverse(a.begin(), a.end());
    vector<int> sfx = compute_pfx();
    reverse(a.begin(), a.end());
    reverse(sfx.begin(), sfx.end());

    if (pfx[n] < m)
    {
        cout << -1 << endl;
        return;
    }

    int end = 0;
    ll ans = 0;

    for (int start = 0; start < n; start++)
    {
        while (end < n && pfx[start] + sfx[end + 1] >= m)
            end++;

        if (pfx[start] + sfx[end] >= m)
            ans = max(ans, query(start, end));
    }

    cout << ans << endl;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
    }
    return 0;
}
