void solve(){
int n,m;
cin>>n>>m;
vector<vector<char>>ar(n,vector<char>(m));
for(int i=0;i<n;i++){
    for(int j=0;j<m;j++){
        cin>>ar[i][j];
    }
}
int a,b,ans=0;cin>>a>>b;
a--;b--;

int dx[8] = {-1, 1, 0, 0, -1, -1, 1, 1};
    int dy[8] = { 0, 0,-1, 1, -1, 1, -1, 1 };

    for (int dir = 0; dir < 8; dir++) {
        int nx = a + dx[dir];
        int ny = b + dy[dir];
        if (nx >= 0 && nx < n && ny >= 0 && ny < m) {
            if (ar[nx][ny] != 'x') {
                cout << "no";
                return;
            }
        }
    }
    cout << "yes";
}
    
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
return 0;
}
