void solve(){
int n;cin>>n;
vector<int>a(9);for(auto &x:a)cin>>x;
int mn=*min_element(a.begin(),a.end()),ln=n/mn;
if(ln==0){cout<<-1;return;}
string ans;ans.reserve(ln);
int rm=n;
for(int i=0;i<ln;++i){
    for(int j=8;j>-1;--j){
        if(rm-a[j]>=(ln-i-1)*mn && rm>=a[j]){
            ans.push_back(char('1'+j));rm-=a[j];break;
        }
    }
}
cout<<ans;
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
        // cout<<endl;
    // }    
return 0;
}
