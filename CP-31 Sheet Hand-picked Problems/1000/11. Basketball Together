#include <bits/stdc++.h>

using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 1e9 + 7;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/
void solve()
{
    ll n, d;
    cin >> n >> d;

    vector<ll> P(n);
    vector<ll> x(n, 0);

    for (auto &a : P)
        cin >> a;

    sort(P.begin(), P.end());

    ll i = 0, j = n - 1, cnt = 0;

    while (i <= j && !x[i])
    {
        x[j] = 1;
        ll sum = P[j];

        while (i <= j && sum <= d && !x[i])
        {
            sum += P[j];
            x[i] = 1;
            i++;
        }

        if (sum > d)
            cnt++;
        j--;
    }

    cout << cnt;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    //   ll Test_Case;
    //   cin >> Test_Case;
    //   while (Test_Case--) {
    solve();
    //     cout << endl;
    //   }

    return 0;
}
