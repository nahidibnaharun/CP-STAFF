#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const ll INF = 1e9 + 7;
const int SIZE = 1e5;
const int MOD = 998244353;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

void solve()
{
    ll n;
    char c;
    string s;
    cin >> n >> c >> s;

    vector<int> ans;
    bool all_match = true;

    for (char x : s)
    {
        if (x != c)
        {
            all_match = false;
            break;
        }
    }

    if (all_match)
    {
        cout << 0 << "\n";
        return;
    }

    bool found = false;
    for (int i = 1; i <= n; i++)
    {
        bool ok = true;
        for (int j = i; j <= n; j += i)
        {
            if (s[j - 1] != c)
            {
                ok = false;
                break;
            }
        }
        if (ok)
        {
            ans.push_back(i);
            found = true;
            break;
        }
    }

    if (!found)
    {
        ans.push_back(n);
        ans.push_back(n - 1);
    }

    cout << ans.size() << "\n";
    for (int i = 0; i < ans.size(); i++)
    {
        cout << ans[i] << (i == ans.size() - 1 ? "" : " ");
    }
    
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    ll Test_Case;
    cin >> Test_Case;
    while (Test_Case--)
    {
        solve();
        cout << endl;
    }

    return 0;
}
