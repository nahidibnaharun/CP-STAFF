void solve()
{
    int n;
    cin >> n;
    vector<int> a(n);
    for (auto &i : a)
    {
        cin >> i;
    }

    vector<int> cnt(n + 1);
    for (auto i : a)
        cnt[i]++;

    if (cnt[0] == 0)
        cout << "NO";
    else if (cnt[1] > 0)
        cout << "YES";
    else
        cout << (cnt[0] == 1 ? "YES" : "NO");
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    // t = 1;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
