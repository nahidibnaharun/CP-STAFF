#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
// const int INF=500;
const ll INF = 2e5+5;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
int n;ll k;cin>>n>>k;
vector<ll>s(n),t(n);
for(auto &x:s)cin>>x;for(auto &x:t)cin>>x;
if(k==0){
    sort(s.begin(),s.end());sort(t.begin(),t.end());
    cout<<(s==t?"YES":"NO");
    return;
}
unordered_map<ll,int>cs,ct;
cs.reserve(n*2);ct.reserve(n*2);
for(ll x:s){
    ll r=x%k,ky=(r==0?0:min(r,k-r));
    cs[ky]++;
}
for(ll x:t){
    ll r=x%k,ky=(r==0?0:min(r,k-r));
    ct[ky]++;
}
bool ans=(cs.size()==ct.size());
if(ans){
    for(auto &x:cs)
    if(ct[x.first]!=x.second){ans=false;break;}
}
cout<<(ans?"YES":"NO");
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }    
return 0;
}
