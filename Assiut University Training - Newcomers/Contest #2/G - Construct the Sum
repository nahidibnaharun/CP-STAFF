#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const ll INF = 1e17+7;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

void solve(){
ll n,s;cin>>n>>s;
vector<ll>ans;
for(ll i=n;i>=1 && s>0;--i){
    if(s-i>=0){
        ans.push_back(i);
        s-=i;
    }
}
if(s!=0)cout<<-1;
else{
    for(size_t i=0 ;i<ans.size();++i){
        cout<<ans[i]<<" ";
    }
}
}
    
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
