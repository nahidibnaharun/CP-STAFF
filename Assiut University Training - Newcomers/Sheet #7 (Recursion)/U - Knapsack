int n,w;
vector<pair<int,int>>wv;
vector<vector<int>>dp;
int f(int i,int cw){
    if(i==n)return 0;
    if(cw<=0)return 0;
    if(dp[i][cw]!=-1)return dp[i][cw];
    int res1=f(i+1,cw);
    int res2=0;
    if(wv[i].first<=cw)res2=wv[i].second+f(i+1,cw-wv[i].first);

    return dp[i][cw]=max(res1,res2);
    
}
void solve(){
cin>>n>>w;
wv.resize(n);
for(auto &x:wv)cin>>x.first>>x.second;
dp.assign(n+1,vector<int>(w+1,-1));
cout<<f(0,w);
 }

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
return 0;
}
