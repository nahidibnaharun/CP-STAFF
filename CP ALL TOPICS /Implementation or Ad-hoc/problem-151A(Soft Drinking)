#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,k,l,c,d,p,nl,np;
    cin>>n>>k>>l>>c>>d>>p>>nl>>np;
    int a= (l*k)/nl,b=p/np,e=c*d;
    
    if(a>b && b<e) cout<<b/n;
    else if(a<b && a<e) cout<<a/n;
    else if (a==b ) cout<<a/n;
    else if(b==c ) cout<<c/n;
    else if(a==c ) cout<<a/n;
    else cout<<e/n;
    return 0;
}
