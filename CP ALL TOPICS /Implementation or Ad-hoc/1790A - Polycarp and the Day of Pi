string s="31415926535897932384626433832795028841971693993751058209749445923078164062862089986280348253421170679";
void solve(){
string x;cin>>x;
int n=x.length();
int ans=0;
for(int i=0;i<n;i++){
    if(s[i]==x[i])ans++;
    else{break;}
}cout<<ans;
cout<<endl;

}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   int t;cin>>t;
   while(t--)
    solve();
} 
