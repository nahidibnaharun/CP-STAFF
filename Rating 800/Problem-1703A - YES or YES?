void solve() {


string s;
cin>>s;
for(int i=0;i<3;i++)s[i]=tolower(s[i]);
(s=="yes")?cout<<"YES":cout<<"NO";
cout<<endl;
return ;

}

int main() {
 int n;
 cin>>n;
 while(n--)
   solve();
 

  return 0;
}
