#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int MOD = (int)(1e9) + 7;
const int MAX = 1e7 + 5;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/


void solve(){
 int n, k;
   cin >> n >> k;
   vector<int> a(n);
   int co = 0;
   for(int i = 0; i < n; ++i){
       cin >> a[i];
       co += a[i] % 2;
   } 
   if(co < k || co % 2 != k % 2){
       cout << "NO";
       return;
   }
   cout << "YES" << endl;
   
   
   for(int i = 0; i < n; ++i){
       if(k == 1) break;
       if(a[i] % 2 == 1){
           cout << i + 1 << " "; 
           --k;
       }
   }
   
   
   cout << n;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
