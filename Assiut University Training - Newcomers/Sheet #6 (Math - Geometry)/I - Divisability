void solve(){
    ll a,b,c;cin>>a>>b>>c;
    if(a>b)swap(a,b);
    if(a%c!=0)a=((a/c)+1)*c;
    if(b%c!=0)b=(b/c)*c;
    
    ll n=(b/c)-(a/c)+1;
    ll ans=(2*a+(n-1)*c);
    if(n%2==0){
        n=n/2;
        ans=ans*n;
    }
    else{
        ans=ans/2;
        ans=ans*n;
    }
    cout<<ans;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
