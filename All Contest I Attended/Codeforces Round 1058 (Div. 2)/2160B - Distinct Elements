#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int INF = 1e9;
const int MOD = 1000000007;
/*
//     
         _   _  __    _   _  _____   _____
//     / | / / / _ \ \ \__\ \\__  _\\  _   \ 
//    /  |/ / / /_\ \ \  __  \  \ \  \ \ \  \ 
//   / /|  / / ____  \ \ \  \ _\_\ \__\ \/  /
//  /_/ |_/ /_/     \__\\_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve() {
    ll n;cin >>n;
    vector<ll>b(n);
    for(auto &x:b)cin >>x;
    vector<ll>a(n);
    a[0]=1;
    ll nv=1;
    for(int i=1;i<n;i++){
        ll df=b[i]-b[i-1] , ind=i+1;
        if(df == ind){
            a[i]=++nv;
        }
        else a[i]=a[i-df];

    }
    for(auto x:a)cout<<x<<" ";
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   
  int t; cin >>t;
  while(t--){
    solve();
    cout<<endl;
  }
return 0;
}
