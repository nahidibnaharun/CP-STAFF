#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
// const int INF=500;
const ll INF = 2e5+5;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
int n;cin>>n;
for(int i=1;i<=n;i++){
    if(i%2==1)cout<<-1;
    else{
        if(i==n)cout<<2;
        else cout<<3;
    }
    if(i<n)cout<<" ";
}

}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }    
return 0;
}
