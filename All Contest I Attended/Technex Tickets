#include<bits/stdc++.h>
using namespace std;
using ll = long long;
/*
ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
	int n;
	cin>>n;
    if(n==1)cout<<1;
    else if(n%2==0)cout<<(n/2)+1;
    else cout<<(n-1)/2;
    cout<<endl;

}

int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int t;
	cin>>t;
	while(t--){
		solve();
	}

}
