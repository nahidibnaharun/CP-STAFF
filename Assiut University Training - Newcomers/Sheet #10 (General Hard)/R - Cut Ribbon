LANG: C++
TASK: practice 
*/


void solve(){
    ll  i,j,a,b,c,n,k,ans,rem;
    cin>>n>>a>>b>>c;
          ans=0;
       for(i=0;i*a<=n;++i)
       {
           for(j=0;i*a+j*b<=n;++j)
           {
                rem=n-i*a-j*b;
                if(rem%c==0)
                {
                    k=rem/c;
                ans=max(ans,i+j+k);}
           }
 
       }
       cout<<ans;

}

    

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
//    ll t;cin>>t;
//    while(t--){
        solve();
//         cout<<endl;
 //   }
return 0;
}
