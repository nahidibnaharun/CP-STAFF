void solve()
{

    int n;
    cin >> n;
    vector<string> a(n);
    for (int i = 0; i < n; i++)
    {
        cin >> a[i];
    }
    string s = "";
    for (int i = 0; i < n; i++)
    {
        string left = a[i] + s;
        string right = s + a[i];
        if (left < right)
        {
            s = left;
        }
        else
        {
            s = right;
        }
    }
    cout << s;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    ll Test_Case;
    cin >> Test_Case;
    while (Test_Case--)
    {
        solve();
        cout << endl;
    }

    return 0;
}
