
#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 200007;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/
void solve()
{
    int x, y;
    cin >> x >> y;

    string s;
    cin >> s;

    int u = 0, d = 0, l = 0, r = 0;

    for (char c : s)
    {
        if (c == 'U')
            u++;
        else if (c == 'D')
            d++;
        else if (c == 'L')
            l++;
        else if (c == 'R')
            r++;
    }

    if (x > 0 && r >= x)
        x = 0;
    if (x < 0 && l >= -x)
        x = 0;
    if (y > 0 && u >= y)
        y = 0;
    if (y < 0 && d >= -y)
        y = 0;

    cout << ((!x && !y) ? "YES" : "NO");
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
