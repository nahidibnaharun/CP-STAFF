#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const int INF=500;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
bool fun(string s){
    int n=s.size();
    for(int i=0;i<n/2;i++){
        if(s[i]!=s[n-i-1])return false;
    }return true;
}
void solve(){  
    int n;cin>>n;
    vector<string>a(n);
    for(auto &x:a)cin>>x;
    unordered_set<string>se,rv2,rv3;
    for(auto x:a){
        if(fun(x)){cout<<"YES";return;}
        int m=x.size();
        string r=x;reverse(r.begin(),r.end());
        if(m==2){
            if(se.count(r) || rv3.count(x)){cout<<"YES";return;}
        }
        else if(m==3){  
                if(se.count(r)){cout<<"YES";return;}
                if(rv2.count(r.substr(0,2))){cout<<"YES";return;}
        }
        se.insert(x);
        if(m==2)rv2.insert(x);
        else{
            string p=x.substr(0,2);reverse(p.begin(),p.end());
            rv3.insert(p);
        }
    }
    cout<<"NO";
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
    solve();
    cout<<endl;
    }
}
