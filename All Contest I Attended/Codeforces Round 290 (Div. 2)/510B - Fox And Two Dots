#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const ll INF = 2e5+5;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
int n,m;string b[51];bool v[51][51],c=false;
int dx[]={1,-1,0,0},dy[]={0,0,1,-1};

void dfs(int x,int y,int px,int py,char col){
    if(x<0||x>=n||y<0||y>=m||b[x][y]!=col) return;
    if(v[x][y]){c=1;return;}
    v[x][y]=1;
    for(int i=0;i<4;i++){
        int nx=x+dx[i],ny=y+dy[i];
        if(nx==px&&ny==py) continue;
        dfs(nx,ny,x,y,col);
    }
}

void solve(){
    cin>>n>>m;
    for(int i=0;i<n;i++) cin>>b[i];
    memset(v,0,sizeof(v));
    for(int i=0;i<n;i++)
        for(int j=0;j<m;j++)
            if(!v[i][j]) dfs(i,j,-1,-1,b[i][j]);
    cout<<(c?"Yes":"No")<<'\n';
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }    
return 0;
}
