#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const int M=200004;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/ 
void solve(){
    int n;cin>>n;
    vector<int>a(n);

    for(auto &x:a)cin>>x;
    vector<int>b(a);
sort(b.begin(),b.end());
for(int i=0;i<n;i++){
    if(a[i]==b[n-1])cout<<a[i]-b[n-2]<<" ";
    else cout<<a[i]-b[n-1]<<" ";
    
}cout<<endl;
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   int t;cin>>t;
   while(t--)
    solve();
} 
