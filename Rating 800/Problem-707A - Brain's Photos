void solve() {
int a,b;
cin>>a>>b;
char ar[a][b];
for(int i=0;i<a;i++){
  for(int j=0;j<b;j++){
    cin>>ar[i][j];
  }
}
for(int i=0;i<a;i++){
  for(int j=0;j<b;j++){
    if(ar[i][j]=='C'||ar[i][j]=='M'||ar[i][j]=='Y'){
      cout<<"#Color"<<endl;
      return ;
    }
  }
}
cout<<"#Black&White"<<endl;
 return;

}

int main() {
//  int n;
//  cin>>n;
//  while(n--)
   solve();
 

  return 0;
}
