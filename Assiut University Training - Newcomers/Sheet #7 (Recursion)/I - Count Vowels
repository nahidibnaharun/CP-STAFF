string s;
int cnt=0;
void f(int i){
   if(i==-1)return;
if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u')cnt++;
f(i-1);
}
void solve(){
getline(cin,s);
transform(s.begin(),s.end(),s.begin(),::tolower);
int i=s.size()-1;
f(i);
cout<<cnt;
 }

