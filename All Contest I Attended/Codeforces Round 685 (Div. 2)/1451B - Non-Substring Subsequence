#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 10000;
const ll MAXN = 2e5 + 5;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

void solve()
{
    int n, Q;
    string s;
    cin >> n >> Q >> s;

    while (Q--)
    {
        int l, r;
        cin >> l >> r;
        l--, r--;

        bool bad = true;

        for (int i = 0; i < l && bad; i++)
            if (s[i] == s[l])
                bad = false;

        for (int i = r + 1; i < n && bad; i++)
            if (s[i] == s[r])
                bad = false;

        if (bad)
            cout << "NO";
        else
            cout << "YES";

        cout << endl;
    }
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        // cout << endl;
    }
    return 0;
}
