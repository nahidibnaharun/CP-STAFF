void solve(){
 int n,m;
 cin>>n>>m;
 vector<vector<char>>a(n,vector<char>(m));
 for(int i=0; i<n; i++)
     for(int j=0; j<m ; j++) cin>>a[i][j];
     
      
 for(int i=0; i<n; i++){
     for(int j=0; j<m; j++){
    if(a[i][j]!='-'){
            if((i+j)%2==0)a[i][j]='W';
            else a[i][j]='B';
    }
 }   
     
    } 
 
 for(int i=0; i<n; i++){
     for(int j=0 ;j<m ;j++)cout<<a[i][j];
     
     cout<<endl;
 }
}

    

