void solve() {
int n,k;
cin>>n>>k;
int a[n];
for(int i=0;i<n;i++)cin>>a[i];
if(is_sorted(a,a+n)||k>1)cout<<"YES";
else cout<<"NO";
cout<<endl;
return ;
}

int main() {
 int n;
 cin>>n;
 while(n--)
   solve();
 

  return 0;
}
