void solve(){
    double l,s1,s2;
    cin >> l >> s1 >> s2;
    double dif= abs(s1-s2);
    int q;
    cin >> q;
    while(q--){
        double s;
        cin >> s;
        cout<<fixed<<setprecision(6)<<(sqrt(2)*l-sqrt(2*s))/dif<<endl;

    }
    
}

    

