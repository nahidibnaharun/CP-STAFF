void solve(){
   int a,b;cin>>a>>b;
   bool chk=true;
   for(int i=a;i<=b;i++){
    string s=to_string(i);
    bool ans=true;
    for(int j=0;j<s.size();j++){
        if(s[j]!='7' && s[j]!='4'){
            ans=false;break;
        }
    }
    if(ans){cout<<i<<" ";chk=false;}
   }
   if(chk)cout<<-1;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
