#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 1006903069;
const int MAX = 1e7 + 10;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

void solve()
{
    int n;
    cin >> n;
    string s;
    cin >> s;

    bool solved = false;
    for (int i = 0; i < n; i++)
    {
        if (s[i] == '0')
        {
            solved = true;
            if (i >= n / 2)
                cout << 1 << " " << i + 1 << " " << 1 << " " << i << endl;
            else
                cout << i + 2 << " " << n << " " << i + 1 << " " << n << endl;
            break;
        }
    }

    if (!solved)
        cout << 1 << " " << n - 1 << " " << 2 << " " << n << endl;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
        solve();

    return 0;
}
