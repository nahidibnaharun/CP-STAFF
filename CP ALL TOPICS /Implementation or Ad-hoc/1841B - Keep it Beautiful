#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 1006903069;
const int MAX = 1e7 + 10;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

void solve() {
        int q;
        cin >> q;

        vector<ll> a;
        a.reserve(q);

        int cnt = 0;
        
        string ans;

        for (int i = 0; i < q; i++) {
            ll x;
            cin >> x;

            int new_cnt = cnt;
            if (!a.empty() && a.back() > x)
                new_cnt++;

            if (new_cnt == 0 || (new_cnt == 1 && x <= a[0])) {
                a.push_back(x);
                cnt = new_cnt;
                ans.push_back('1');
            } else {
                ans.push_back('0');
            }
        }

        cout << ans ;

}

int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

  int t;
  cin >> t;
  while (t--){
    solve();
    cout<<endl;
  }

  return 0;
}
