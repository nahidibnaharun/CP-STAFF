vector<int>a;
bool f(ll n,int i){
    if(i==n/2)return true;
  if(a[i]!=a[n-1-i])return false;
  else return f(n,i+1);
}
void solve(){
ll n;cin>>n;
a.resize(n);
for(auto &x:a)cin>>x;
int i=0;
if(f(n,i))cout<<"YES";
else cout<<"NO";
 }

