int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int d;
    cin >> d;
    {
        string s = to_string(d);

        for (int i = 0; i < d; ++i)
        {
            cout << s;
        }
        cout << endl;
    }

    return 0;
}
