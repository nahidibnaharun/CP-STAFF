#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int INF = 1e9;
const int MOD = 1000000007;
/*
//     
         _   _  __    _   _  _____   _____
//     / | / / / _ \ \ \__\ \\__  _\\  _   \ 
//    /  |/ / / /_\ \ \  __  \  \ \  \ \ \  \ 
//   / /|  / / ____  \ \ \  \ _\_\ \__\ \/  /
//  /_/ |_/ /_/     \__\\_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve() {
    int n;
    cin >>n;
    vector<ll>a(n);
    for(auto &x:a)cin >>x;
    sort(a.begin(),a.end());
    
    
    vector<ll>na;
    int left=0,right=n-1;
    while(left<=right){
        if(left==right)na.push_back(a[left]);
        else{
        na.push_back(a[left]);
        na.push_back(a[right]);
        }
        left++;right--;
    }
    reverse(na.begin(),na.end());

    for(auto x:na)cout<<x<<" ";
   
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   
  int t; cin >>t;
  while(t--){
    solve();
    cout<<endl;
  }
return 0;
}
