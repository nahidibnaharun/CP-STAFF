#include <bits/stdc++.h>

using namespace std;

using ll = long long;

/*
ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    int n;
    ll a1=0,a2=0;cin>>n;
    vector<int> a(n);
    for(auto &x:a){
        cin>>x;
        a1+=x*x;
        a2+=x;
    }
    cout<<n*a1-a2*a2;
   return 0;
    
}
