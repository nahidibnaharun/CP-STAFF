void solve(){
int n;cin>>n;vector<int>a(n),b(n);
for(int i=0;i<n;i++){cin>>a[i];b[i]=a[i];}
sort(b.begin(),b.end());
for(int i=0;i<n;i++){
    if((a[i]%2)!=(b[i]%2)){cout<<"NO";return;}
}cout<<"YES";
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--){
  solve();
  cout<<endl;
  }
}
