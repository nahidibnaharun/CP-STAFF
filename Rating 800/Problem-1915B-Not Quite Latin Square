#include<bits/stdc++.h>
using namespace std;
using ll = long long;
/*
ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

void solve(){
char a[3][3];
int b[3]={0};
for(int i=0;i<3;i++){
    for(int j=0;j<3;j++){
        cin>>a[i][j];
        if(a[i][j]=='A')b[0]++;
        else if(a[i][j]=='B')b[1]++;
        else if(a[i][j]=='C')b[2]++;
    }
}
if(b[0]==2)cout<<"A";
else if(b[1]==2)cout<<"B";
else if(b[2]==2)cout<<"C";
cout<<endl;
return;
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int t;
	cin>>t;
	while(t--)
	solve();
return 0;
}   
