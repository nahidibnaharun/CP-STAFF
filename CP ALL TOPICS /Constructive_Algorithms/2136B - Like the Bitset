#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const ll INF = 1e17;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
   int n,k;string s;cin>>n>>k>>s;
   int mr=0,cr=0,cnt1=0;
   for(char x:s){
    if(x=='1'){cr++;cnt1++;}
    else{mr=max(mr,cr);cr=0;}
   }
   mr=max(mr,cr);
   if(mr>=k){cout<<"NO";return;}
   cout<<"YES"<<endl;
   vector<int>p(n);
   int ps=1;
   for(int i=0;i<n;++i)if(s[i]=='1')p[i]=ps++;
   ps=cnt1+1;
   for(int i=0;i<n;++i)if(s[i]=='0')p[i]=ps++;
   for(auto x:p)cout<<x<<" ";
 }
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
