void solve(){
string s;cin>>s;
string l="EGYPT";
int n=s.size();

    for(int i=0;i<n;){
        if( i+4 <n && s.substr(i,5) == l){
            cout<<" ";i+=5;
        }
        else{
            cout<<s[i];i++;
        }
    }
}
