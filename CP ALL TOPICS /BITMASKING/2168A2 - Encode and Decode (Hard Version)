string encode(int x){
    string s(7,'a'); 
    for(int i=0 ;i<7 ;i++){s[i] = x%26+'a'; x/=26;}
    return s;
}

int decode(string s){
    int x=0;
    for(int i=6;i>=0 ;i--)x = x*26+(s[i]-'a');
    return x;
}

void solve(){
    string t; cin >> t; 
    if(t=="first"){
        int n; cin >> n; 
        for(int i=0 ; i<n; i++){int x; cin >> x; cout << encode(x);}
    }
    else{
        string s; cin >> s; cout << s.size() /7<<" ";
        for(int i=0 ;i<s.size() ;i+=7){
            string x= s.substr(i,7); cout << decode(x)<<" ";
        }
    }
}


int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

//   ll Test_Case; cin >> Test_Case;
//   while (Test_Case--) {
  solve();
//   cout<<endl;
//   }

  return 0;
}
