ll fun(int a,int b){
    if(a==b)return 1;
    if(a>b)return 0;
    return fun(a+1,b)+fun(a+2,b)+fun(a+3,b);
}

void solve(){
    int a,b;cin>>a>>b;
    if(a==b)cout<<1;
    else
    cout<<fun(a,b);
}
