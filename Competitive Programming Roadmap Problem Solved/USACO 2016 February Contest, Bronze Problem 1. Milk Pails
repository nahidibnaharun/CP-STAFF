#include<bits/stdc++.h>
using namespace std;
using ll = long long;
/*
ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    // freopen("pails.in", "r", stdin); 
    // freopen("pails.out", "w", stdout); 
    int x,y,m;
    cin>>x>>y>>m;
    int maximum=0,total=0;
    for(int i=1;i<=m/x;i++){
        for(int j=1;j<=m/y;j++){
            total=i*x+j*y;
            if(total<=m)
            maximum=max(maximum,total);
        }
    }
    cout<<maximum;
	return 0;
}
