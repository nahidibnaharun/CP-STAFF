#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int INF = 1e9;
const int MOD = 1000000007;
/*
//     
         _   _  __    _   _  _____   _____
//     / | / / / _ \ \ \__\ \\__  _\\  _   \ 
//    /  |/ / / /_\ \ \  __  \  \ \  \ \ \  \ 
//   / /|  / / ____  \ \ \  \ _\_\ \__\ \/  /
//  /_/ |_/ /_/     \__\\_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

void solve(){
    int n,m;
    string s;
    cin >>n>>m>>s;
    set<int>a;
    for(int i=0;i<m;i++){
        int x;cin >>x;
        a.insert(x);
    }
    int cur=1;
    for(char x:s){
        cur++;
        if(x=='B')while(a.count(cur))cur++;

        a.insert(cur);
        if(x=='B')while(a.count(cur))cur++;
    }

    cout<<a.size()<<endl;
    for(auto x:a)cout<<x<<" ";
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   
   int t;
   cin >>t;
   while(t--){
    solve();
    cout<<endl;
   }
return 0;
}
