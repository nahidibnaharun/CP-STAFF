#include <bits/stdc++.h>

using namespace std;

int main()
{int r,c,ar[5][5];
   for(int i=0;i<5;i++){
       for(int j=0;j<5;j++){
           cin>>ar[i][j];
           if(ar[i][j]==1){
               r=i+1,c=j+1;
           }
       }
   }
   r = abs(r-3);
   c = abs(c-3);
cout<<r+c<<endl;
    return 0;
}
