void solve(){
int n;cin>>n;
vector<int>a(n);
for(auto &x:a)cin>>x;
    int s=0;
for(auto x:a)s+=abs(x);
    cout<<s;
cout<<endl;
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   int t;cin>>t;
   while(t--)
    solve();
} 
