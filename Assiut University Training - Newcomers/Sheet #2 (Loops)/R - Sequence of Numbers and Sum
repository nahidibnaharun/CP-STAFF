void solve(){
   while(true){
    int a,b;cin>>a>>b;
    ll ans=0;
    if(a<=0 || b<=0)return;
    for(int i=min(a,b);i<=max(a,b);i++){
        cout<<i<<" ";
        ans+=i;
    }
    cout<<"sum ="<<ans<<endl;
   }

}
