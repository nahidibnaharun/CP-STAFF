#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
 void solve(){
    int n;cin>>n;
    int a[n+1];
    for(int i=1;i<=n;i++)cin>>a[i];
    for(int i=1;i<n;i++)if((2*min(a[i],a[i+1])>max(a[i],a[i+1]))){cout<<"YES"<<endl;return;}
    cout<<"NO";
    cout<<endl;
 }
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    int t;cin>>t;
    while(t--)
    solve();
}

