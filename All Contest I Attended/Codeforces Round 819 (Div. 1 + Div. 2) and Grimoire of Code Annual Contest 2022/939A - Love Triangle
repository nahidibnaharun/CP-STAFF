void solve(){
	ll n;cin>>n;
	vector<ll>a(n+1);
	for(int i=1;i<=n;i++)cin>>a[i];
	int ok=0;
	for(int i=1;i<=n;i++){
        ll b=a[i];
        ll c=a[b];
        ll d=a[c];
        if(d==i){
        	ok=1;
        	break;
        }
	}
	cout<<(ok?"YES":"NO");
}



int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

