void solve(){
    int a[4];
    for(int i=0;i<4;i++)cin>>a[i];
    string s;
    cin>>s;
    int ct=0;
    for(int i=0;i<s.size();i++){
        ct+=a[s[i]-'1'];
    }
    cout<<ct;
cout<<endl;
}
