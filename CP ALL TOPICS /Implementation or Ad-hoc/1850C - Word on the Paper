LANG: C++
TASK: practice 
*/
 void solve(){
    char ar[8][8];
    string s;
    for(int i=0;i<8;i++){
        for(int j=0;j<8;j++){
            cin>>ar[i][j];
            if(ar[i][j]!='.')s.push_back(ar[i][j]);
        }
    }
    cout<<s;
    cout<<endl;
 }
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    int t;cin>>t;
    while(t--)
    solve();
}
