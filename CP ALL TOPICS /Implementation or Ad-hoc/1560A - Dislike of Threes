int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
    int hs[1001];
    for(int i=1,j=1;j<=1001;i++){
        if(i%3!=0 && i%10!=3){
            hs[j]=i;
            j++;
        }

    }
    int t;cin>>t;
    while(t--){
        int x;
        cin>>x;
        cout<<hs[x]<<endl;
    }
   return 0;   
}


