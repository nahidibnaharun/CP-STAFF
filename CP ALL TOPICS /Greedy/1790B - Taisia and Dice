#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 998244353;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/


void solve()
{
     ll n, s, r;
    cin >> n >> s >> r;

    vector<ll> v(n);
    ll t = s - r;         
    v[0] = t;

    ll base = r / (n - 1);

    for (ll i = 1; i < n; i++)
        v[i] = base;

    sort(v.begin(), v.end());

    ll rem = r % (n - 1);
    ll i = 0;

    while (rem--)
    {
        v[i]++;
        i++;
    }

    for (auto &u : v)
        cout << u << " ";
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    ll Test_Case;
    cin >> Test_Case;
    while (Test_Case--)
    {
        solve();
        cout << endl;
    }

    return 0;
}
