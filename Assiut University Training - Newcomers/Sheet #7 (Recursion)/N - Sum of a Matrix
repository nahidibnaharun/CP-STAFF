vector<vector<int>> a,b;
void f(int i,int j, int n, int m){
  if(i==n)return;
    cout<<a[i][j]+b[i][j];
  if(j==m-1) {cout<<endl;f(i+1,0,n,m);}
  else {cout<<" ";f(i,j+1,n,m);}
}
void solve(){
int n,m;cin>>n>>m;
a.resize(n,vector<int>(m));
b.resize(n,vector<int>(m));
for(int i=0;i<n;i++){
    for(int j=0;j<m;j++){
        cin>>a[i][j];
    }
}
for(int i=0;i<n;i++){
    for(int j=0;j<m;j++){
        cin>>b[i][j];
    }
}
int i=0,j=0;
f(i,j,n,m);
 }

int main(){
    ios_base::sync_with_stdio(false);
