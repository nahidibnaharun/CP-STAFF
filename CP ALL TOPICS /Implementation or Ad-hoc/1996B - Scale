void solve(){
int n,k;cin>>n>>k;
char ar[n][n];
for(auto &x:ar){
    for(char &c:x)cin>>c;
    }
for(int i=0;i<n;i+=k){
    for(int j=0;j<n;j+=k)cout<<ar[i][j];
    cout<<endl;
}
    // cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
  int t;cin>>t;
  while(t--)
    solve();
} 

