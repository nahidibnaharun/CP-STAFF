void solve(){
int n;cin>>n;set<int>a;
for(int i=1;i*i<=n;i++)a.insert(i*i);
for(int i=1;i*i*i<=n;i++)a.insert(i*i*i);
cout<<a.size();
    cout<<endl;
} 

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--)
    solve();
}
