void solve() {
    int n;cin >>n;
    vector<int>a(n); for(auto &x:a)cin >> x;
    int mx=0;
    for(int i=0 ;i<n-1;i++)mx=max(mx,a[i]);
    cout<<mx+a[n-1];
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   
   int t;
   cin >>t;
   while(t--){
    solve();
    cout<<endl;
   }
return 0;
}
