void solve(){
    int n, m, i=2, prime = 0;
    cin >> n >> m;
 
    for(i=n+1; i<m; i++) {
        int j=2;
        for(j=2; j<i; j++) {
            if(i%j==0) break;
        }
        if(j==i) prime++;
    }
    for(int i=2; i<m; i++) if(m%i==0) prime++;
 
    if(prime==0) cout << "YES";
    else cout << "NO";
 
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // ll t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
return 0;
}
