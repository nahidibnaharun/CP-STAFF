void solve() {
    int n, k;
    cin >> n >> k;
    
    vector<double> vc(n);
    for (auto &x : vc) cin >> x;
    
    double max_avg = -1e18;

    auto calc_max_avg = [&](int len) {
        double sum = 0;
        for (int i = 0; i < len; i++) sum += vc[i];
        max_avg = max(max_avg, sum / len);
        for (int i = len; i < n; i++) {
            sum += vc[i] - vc[i - len];
            max_avg = max(max_avg, sum / len);
        }
    };

    for (int len = k; len <= n; len++) calc_max_avg(len);
    
    cout << fixed << setprecision(10) << max_avg;
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    
    // ll Test_Case; cin >> Test_Case;
    // while (Test_Case--) {
        solve();
        //cout<<endl;
    // }

    return 0;
}
