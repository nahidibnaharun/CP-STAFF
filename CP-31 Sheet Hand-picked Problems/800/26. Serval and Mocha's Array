#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int INF = 1e9;
const int MOD = 1e9+7;
/*

         _   _  __    _   _  _____   _____
    / | / / / _ \ \ \__\ \\__  _\\  _   \
   /  |/ / / /_\ \ \  __  \  \ \  \ \ \  \
  / /|  / / ____  \ \ \  \ _\_\ \__\ \/  /
 /_/ |_/ /_/     \__\\_\  \_\\______\\___/
 
ID: mdnahidibnaharun
LANG: C++ 
TASK: Codeforces Round 1061 (Div. 2)
*/




void solve(){
    int n; cin >> n;
    int a[n]; for(auto &x : a)cin >> x;
    bool chk = false;

    for(int i=0 ; i<n ;i++){
        for(int j=i+1 ;j<n ;j++){
            if(__gcd(a[i],a[j]) <=2)chk =true;
        }
    }
    if(chk)cout<<"Yes";
    else cout<<"No";
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    
    


    ll Test_Case;
    cin >> Test_Case;
    while(Test_Case--){
        solve();
        cout<<endl;
    }
    return 0;
}
