void solve(){
int a[3];
int b,c=0;
cin>>b;
for(int i=0;i<3;i++){
    cin>>a[i];
    if(b<a[i])c++;
}
cout<<c<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
    int t;cin>>t;
    while(t--)
    solve();
   return 0;   
}


