#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const int MAX=200007,MOD=1000000007;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
    int n,r=0,i=1;cin>>n;
    while(i<=n){
        if(i<10)i++;
        else if(i<100)i+=10;
        else if(i<1000)i+=100;
        else if(i<10000)i+=1000;
        else if(i<100000)i+=10000;
        else if(i<1000000)i+=1000000;
        r++;
    }cout<<r;
cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--)
    solve();
}
