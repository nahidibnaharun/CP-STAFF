#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;

const ll INF = 1000000007;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve() {
    string s;cin>>s;int n=s.size();
    for(int i=0;i<n-1;i++){
        int a=s[i]-'0',b=s[i+1]-'0';
        if(a+b>10){
            cout<<s.substr(0,i);
            cout<<a+b;
            cout<<s.substr(i+2,n-1);
            break;
        }
    }
      
    int a=s[0]-'0',b=s[1]-'0';
    cout<<a+b<<s.substr(2,n-1);
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
    solve();
    cout<<endl;
    }
}
