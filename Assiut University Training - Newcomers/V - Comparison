void solve(){
string s;getline(cin,s);
size_t pos=s.find_first_of("<>=");
string s1=s.substr(0,pos-1),s2=s.substr(pos+2);
int n1=stoi(s1),n2=stoi(s2);
char op=s[pos];
if (op == '>') {
        if(n1>n2)cout<<"Right";
        else cout<<"Wrong";
    } else if(op=='<'){
        if(n1<n2)cout<<"Right";
        else cout<<"Wrong";
    } else if(op=='='){
        if(n1==n2)cout<<"Right";
        else cout << "Wrong";
    }
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
return 0;
}
