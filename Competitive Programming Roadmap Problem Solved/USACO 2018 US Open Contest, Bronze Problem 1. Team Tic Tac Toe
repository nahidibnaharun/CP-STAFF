#include<bits/stdc++.h>
using namespace std;
using ll = long long;
/*
ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // freopen("tttt.in", "r", stdin); 
    // freopen("tttt.out", "w", stdout); 

    char tt[3][3];
    int one=0,two=0;
    vector<string> d(8);
    for(int i=0;i<3;i++)for(int j=0;j<3;j++){
        cin>>tt[i][j];
        if(i==j)d[0].push_back(tt[i][j]);
        if(i+j==2)d[1].push_back(tt[i][j]);
        if(i==0)d[2].push_back(tt[i][j]);
        if(i==1)d[3].push_back(tt[i][j]);
        if(i==2)d[4].push_back(tt[i][j]);
        if(j==0)d[5].push_back(tt[i][j]);
        if(j==1)d[6].push_back(tt[i][j]);
        if(j==2)d[7].push_back(tt[i][j]);
        }
set<string> s(d.begin(), d.end()); 


    for(auto &x: s){
        int a=x[0],b=x[1],c=x[2];
        if(a==b && b==c)one++;
        else if(a==b || b==c || a==c)two++;
    }
    cout<<one<<endl<<two;




    

    return 0;
}
