void solve(){
    int n;string s;cin>>n>>s;
    if(n>2 || s=="11" || s== "00")cout<<"NO";
    else cout<<"YES";
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--){
    solve();
    cout<<endl;
  }
} 
