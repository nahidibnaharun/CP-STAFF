#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int MOD = (int)(1e9) + 7;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

void solve(){
int n;cin>>n;
vector<int>f(n+1,0);
for(int i=0;i<n;i++){
    int x;cin>>x;
    if(x>=1 && x<=n)f[x]++;
}
int ans=0;
for(int i=1;i<=n;i++){
    int c=0;
    for(int j=1;j<=n;j++)if(f[j]>=i)c++;
    ans=max(ans,c*i);
}
cout<<ans;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
