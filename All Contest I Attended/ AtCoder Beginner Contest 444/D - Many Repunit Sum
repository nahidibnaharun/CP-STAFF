#include <bits/stdc++.h>
#include <unordered_map>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 1006903069;
const int MAX = 1e7 + 10;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

void solve() {
  int n;
  cin >> n;
  vector<int> a(n);
  int mxa = 0;
  for (int i = 0; i < n; i++) {
    cin >> a[i];
    mxa = max(mxa, a[i]);
  }
  vector<ll> fq(mxa + 2, 0);
  for (int x : a)
    fq[x]++;

  vector<ll> s(mxa + 2, 0);
  for (int k = mxa; k >= 1; k--) {
    s[k] = s[k + 1] + fq[k];
  }
  vector<int> dg;
  ll cr = 0;
  for (int k = 1; k <= mxa; k++) {
    ll cur = s[k] + cr;
    dg.push_back(cur % 10);
    cr = cur / 10;
  }
  while (cr > 0) {
    dg.push_back(cr % 10);
    cr /= 10;
  }
  for (int i = dg.size() - 1; i >= 0; i--)
    cout << dg[i];
  cout << endl;
}

int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

  //   int t;
  //   cin >> t;
  //   while (t--){
  solve();
  //     cout<<endl;
  //   }

  return 0;
}
