#include<bits/stdc++.h>
using namespace std;
using ll = long long;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
int n;cin>>n;
vector<ll>ar(n);for(auto &x:ar)cin>>x;
int m=0,h=n-1;
while(m<=h){
    if(m!=h)
    cout<<ar[m]<<" "<<ar[h]<<" ";
    else cout<<ar[m];
    m++;h--;
}
cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL); 
    int t;
    cin>>t;
    while(t--)
    solve();
    return 0;   
}
