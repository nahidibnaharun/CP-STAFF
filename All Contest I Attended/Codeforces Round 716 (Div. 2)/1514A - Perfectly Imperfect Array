
#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 10000;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

bool sq[MAX + 5];
void solve()
{

    int n;
    cin >> n;

    bool ok = true;

    while (n--)
    {
        int a;
        cin >> a;
        ok &= sq[a];
    }

    cout << (ok ? "NO" : "YES");
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    for (int i = 1; i * i <= MAX; i++)
        sq[i * i] = true;
    int t;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
