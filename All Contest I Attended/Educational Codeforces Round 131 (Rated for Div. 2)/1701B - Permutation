void solve(){
    ll n,x=0; cin >>n;
    vector<ll>a(n+1,0);
    cout<<2<<endl<<1<<" ";
    for(int i=2;i<=n ;i++){
        if(a[i]==0){
            for(int j=i;j<=n ;j*=2){
                if(a[j]==0){
                    cout<<j<<" ";a[j]++;
                }
            }
        }
    }
       

}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    
    


    ll Test_Case;
    cin >> Test_Case;
    while(Test_Case--){
        solve();
        cout<<endl;
    }
    return 0;
}
