void solve(){
    string s;
    cin >> s;
    int n=s.size(),l,m=0,sn=0;
    cin >>l;
    for(auto x : s){
        if(x=='?' || x=='*')m++;
        if(x=='*')sn++;
        }
    if(!(n-2*m <= l)){
        cout<<"Impossible";
        return;
    }

    if(n-m < l){
        if(sn==0){
            cout<<"Impossible";
            return;
        }
        int lp= l-(n-m);
        
        int i=0;
        for(;i<n;i++){
            if(!(s[i]=='?' || s[i]=='*'))cout<<s[i];
            else if(s[i]=='*')break;
        }
        while(lp--){
            cout<<s[i-1];
        }
        for(;i<n;i++){
            if(!(s[i]=='?' || s[i]=='*'))cout<<s[i];
        }
    }
    else{
        int lp = (n-m)-l;

        for(int i=0 ; i<n;i++){
            if(lp==0){
                if(!(s[i]=='?' ||s[i]=='*'))cout<<s[i];
            }
            else if(!(s[i]=='?' ||s[i]=='*')){
                if(i<n && (s[i+1]=='?'|| s[i+1]=='*')){
                    lp--;
                }
                else{
                    cout<<s[i];
                }
            }
        }
    }
    
}

    

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // ll t;cin>>t;
    // while(t--){
        solve();
        // cout<<endl;
    // }
return 0;
}
