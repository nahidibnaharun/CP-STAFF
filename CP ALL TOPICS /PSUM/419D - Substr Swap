void solve(){
int n,m;cin>>n>>m;string s,t;cin>>s>>t;
vector<int>df(n+1,0);
for(int i=0;i<m;++i){
    int l,r;cin>>l>>r;--l;--r;
    df[l]++;df[r+1]--;
}
string ans;ans.resize(n);
int cr=0;
for(int i=0;i<n;++i){
    cr+=df[i];
    if((cr&1)==0)ans[i]=s[i];
    else ans[i]=t[i];
}cout<<ans;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
        // cout<<endl;
    // }
return 0;
}
