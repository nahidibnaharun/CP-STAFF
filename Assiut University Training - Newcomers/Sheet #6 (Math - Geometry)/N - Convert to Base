LANG: C++
TASK: practice 
*/

void solve(){
int n;cin>>n;
if(n==1){
    string x;cin>>x;
    ll a;cin>>a;
    ll i=1,ans=0;
    for(int j=x.size()-1;j>-1;j--){
        ll y;
        if(x[j]>='0' && x[j]<='9')y= x[j]-'0';
        else if(x[j] >='A' && x[j]<='Z')y = x[j] - 'A' +10;
        ans +=(y*i);
        i*=a;
    }
    cout<<ans;
}
else{
ll a, b; cin >> a >> b;
string ans = "";
if(a==0) ans="0";
while (a != 0) {
    ll d = a % b;
    char c;
    if (d < 10) c = d + '0';
    else c = d - 10 + 'A';
    ans += c;
    a /= b;
}
reverse(ans.begin(), ans.end());
cout << ans;

}
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // int t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
return 0;
}
