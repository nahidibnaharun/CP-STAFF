vector<int>a;
void fun(int n){
    vector<int>c;
    for(int i=0;i<n;i++){
    if(c.empty())c.push_back(a[i]);
    else{
        if(c.back()!=a[i])c.push_back(a[i]);
    }
    }
    cout<<c.size();
}
void solve(){
int n;cin>>n;
a.resize(n);
for(auto &x:a)cin>>x;
sort(a.begin(),a.end());
fun(n);
}


