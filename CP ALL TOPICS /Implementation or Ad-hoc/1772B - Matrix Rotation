void solve(){
    int a,b,c,d;cin>>a>>b>>c>>d;
    if(a>b && c>d || a<b && c<d){
        if(a>c && b>d || a<c && b<d)cout<<"YES";
        else cout<<"NO";
    }
    else cout<<"NO";
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--){
    solve();
    cout<<endl;
  }
} 
