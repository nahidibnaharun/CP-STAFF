#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int INF = 1e9;
const int MOD = 1000000007;
/*
//

         _   _  __    _   _  _____   _____
//     / | / / / _ \ \ \__\ \\__  _\\  _   \ 
//    /  |/ / / /_\ \ \  __  \  \ \  \ \ \  \ 
//   / /|  / / ____  \ \ \  \ _\_\ \__\ \/  /
//  /_/ |_/ /_/     \__\\_\  \_\\______\\___/
 
ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/
void solve() {
ll n,m;
cin>>n>>m;
unordered_map<ll,int>a;
    for(int i=0;i<n;i++){
        ll x;cin>>x;
        a[x%m]++;
    }
int ans=0;
if(a.count(0)){
    ans++;
}
for(ll r=1 ;r<=m/2;r++){
    if(r*2==m){
        if(a.count(r))ans++;
    continue;
    }


    ll pat=m-r;
    if(!a.count(r) && !a.count(pat)){
        continue;
    }
    int c1=a.count(r)?a[r]:0 , c2=a.count(pat)?a[pat]:0;

    if(c1==0){
        ans+=c2;
        continue;
    }
    if(c2==0){
        ans+=c1;
        continue;
    }
    int mnc=min(c1,c2), mxc=max(c1,c2);
    if(mxc-mnc<=1)ans++;
    else ans+=(mxc-mnc);
}
cout<<ans;
}

int main(){
ios_base::sync_with_stdio(false);
cin.tie(NULL);

int t; cin >>t;
while(t--){
solve();
cout<<endl;
}
return 0;
}
