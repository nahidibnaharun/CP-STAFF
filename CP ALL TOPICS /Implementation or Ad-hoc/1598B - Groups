
#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 300005;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

void solve()
{
    int n;
    cin >> n;

    vector<vector<int>> a(n, vector<int>(5));
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < 5; j++)
        {
            cin >> a[i][j];
        }
    }

    bool ans = false;

    for (int j = 0; j < 5; j++)
    {
        for (int k = 0; k < 5; k++)
        {
            if (j == k)
                continue;

            int cnt1 = 0, cnt2 = 0, cntno = 0;

            for (int z = 0; z < n; z++)
            {
                if (a[z][j] == 1)
                    cnt1++;
                if (a[z][k] == 1)
                    cnt2++;
                if (a[z][j] == 0 && a[z][k] == 0)
                    cntno++;
            }

            if (cnt1 >= n / 2 && cnt2 >= n / 2 && cntno == 0)
            {
                ans = true;
            }
        }
    }

    if (ans)
        cout << "YES";
    else
        cout << "NO";
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
