void solve(){
    int n;cin>>n;
    ll ar[n];
    for(auto &x:ar)cin>>x;
    ll x;cin>>x;
    for(int i=0;i<n;i++){
        if(x==ar[i]){
            cout<<i;return;
        }
    }
    cout<<-1;
}
    
