bool fun(ll id,ll sum,vector<ll> &a,ll x){
    if(id==a.size())return x==sum;
    return fun(id+1,sum+a[id],a,x)||fun(id+1,sum-a[id],a,x);
}
void solve(){
    ll n,x;cin>>n>>x;
    vector<ll>a(n);for(auto &y:a)cin>>y;
    
   if(fun(1,a[0],a,x))cout<<"YES";
   else cout<<"NO";
}

