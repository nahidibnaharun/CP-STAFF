ll a[N][N], ans[N];
void solve(){
string s;cin>>s;
int n=s.length();
char mc='z'+1;
for(int k=0;k<n;k++){
    if(mc<s[k])cout<<"Ann";
    else cout<<"Mike";
    
    cout<<endl;
    mc=min(mc,s[k]);
}
cout<<endl;
} 
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
//   int t;cin>>t;
//   while(t--)
    solve();
} 
