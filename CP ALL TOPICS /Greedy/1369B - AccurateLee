#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int INF = 1e9;
const int MOD = 1000000007;
/*
//     
         _   _  __    _   _  _____   _____
//     / | / / / _ \ \ \__\ \\__  _\\  _   \ 
//    /  |/ / / /_\ \ \  __  \  \ \  \ \ \  \ 
//   / /|  / / ____  \ \ \  \ _\_\ \__\ \/  /
//  /_/ |_/ /_/     \__\\_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve() {
   int n; cin >>n;
   string s; cin >>s;
   int fo=-1,lz=-1;
   for(int i=0;i<n;i++){
    if(s[i]=='1'){
        fo=i;
        break;
    }
   }

   for(int i=n-1;i>=0 ;i--){
    if(s[i]=='0'){
        lz=i;
        break;
    }
   }

   if(fo==-1 || lz==-1 || fo>lz){
    cout<<s;return;
   }
   string pfx= s.substr(0,fo);
   string sfx=s.substr(lz+1);
   cout<<pfx<<"0"<<sfx;

}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   
  int t; cin >>t;
  while(t--){
    solve();
    cout<<endl;
  }
return 0;
}
