void solve() {
    string s;cin>>s;int n=s.length();
    int ar[26]={0};for(auto x:s)ar[x-'a']++;
    char a='a';
    for(int i=0;i<26;i++){
        int x=ar[i];
        while(x--)cout<<a;
        a++;
    }   
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
    solve();
    cout<<endl;
    }
}
