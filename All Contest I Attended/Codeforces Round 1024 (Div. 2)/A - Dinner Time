 void solve(){
   ll n,m,p,q;
   cin>>n>>m>>p>>q;
   if(n%p ==0 && m!=(n/p)*q)cout<<"NO";
   else cout<<"YES";
   
cout<<endl;
 }
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    ll t;cin>>t;
    while(t--)
    solve();    
}
