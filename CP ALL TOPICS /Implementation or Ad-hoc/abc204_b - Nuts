int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    int n;cin>>n;
    int ans=0;
    while(n--){
        int x;
        cin>>x;
        if(x>10)ans+=(x-10);
    }
    cout<<ans;
   return 0;
    
}
