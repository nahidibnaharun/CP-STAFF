void solve(){
    int a, b, final = 0, cnt = 0, first = 0, second = 0;
  cin >> a >> b;
  char c[a][b];
  for (int i = 0; i < a; i++) {
    for (int j = 0; j < b; j++) {
      cin >> c[i][j];
    }
  }
 
  for (int i = 0; i < a; i++) {
      cnt=0;
    for (int j = 0; j < b; j++) {
      if (c[i][j] == 'S') {
        cnt = 0;
        break;
      } else {
        cnt++;
      }
    }
    if (cnt == b) {
      first++;
    }
    final += cnt;
  }
 
  for (int i = 0; i < b; i++) {
      cnt=0;
    for (int j = 0; j < a; j++) {
      if (c[j][i] == 'S') {
        cnt = 0;
        break;
      } else {
        cnt++;
      }
    }
    if (cnt == a) {
      second++;
    }
    final += cnt;
  }
  cout << final - first * second;

}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // ll t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
return 0;
}
