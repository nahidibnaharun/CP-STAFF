void solve(){
int x,y,n;cin>>x>>y>>n;
if(n-n%x+y<=n)cout<<n-n%x+y;
else cout<<n-n%x-(x-y);

cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
    int t;cin>>t;
    while(t--){
solve();
    }
    
    return 0;   
}
