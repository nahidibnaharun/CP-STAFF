void solve(){
 int n;cin>>n;
 int ans=0;
 for(int i=1;i<=n;i*=10)ans+=min(9,n/i);
 cout<<ans;
cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--)
    solve();
}
