#include<bits/stdc++.h>
using namespace std;
using ll = long long;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
    int n;
    cin>>n;
    if(n%2!=0){
        for(int i=1;i<=n;i++){
            cout<<(2*(i-1))%n+1<<" ";
        }
    }
    else cout<<-1;
    cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
int t;
cin>>t;
while(t--)
solve();
    return 0;   
}
