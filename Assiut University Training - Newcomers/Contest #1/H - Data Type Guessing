void solve(){
   ull n,k,a;cin>>n>>k>>a;
   ull mult=n*k;
   if(mult%a !=0){cout<<"double";return;}
   ull rs=mult/a;
   if(rs<=INT_MAX)cout<<"int";
   else if(rs<=LLONG_MAX)cout<<"long long";
   else cout<<"double";


}
