void solve(){
    int n;
    cin >> n;
    unordered_map< int , int > frq;
    vector< int > df(n);
    for(auto & x : df){
        cin >> x;
        frq[x]++;
    }
    int rpl = 0;
    for(auto &x: frq){
        if(x.second > 1) rpl +=x.second - 1;
    }
    if(rpl == 0) cout << -1;
    else cout<< rpl;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // ll t;cin>>t;
