const int INF = 1e9;
const int SIZE = 1e5 + 5;
const int MOD = 1006903069;
const int MAX = 1e7 + 10;

int n, m, k;
int a[SIZE];

void solve()
{
    cin >> n >> m >> k;

    for (int i = 1; i <= k; i++)
        cin >> a[i];

    bool flag = false;
    ll tot = 0;

    for (int i = 1; i <= k; i++)
    {
        if (a[i] / n > 2)
            flag = true;
        if (a[i] / n >= 2)
            tot += a[i] / n;
    }

    if (tot >= m && (flag || m % 2 == 0))
    {
        cout << "Yes";
        return;
    }

    flag = false;
    tot = 0;

    for (int i = 1; i <= k; i++)
    {
        if (a[i] / m > 2)
            flag = true;
        if (a[i] / m >= 2)
            tot += a[i] / m;
    }

    if (tot >= n && (flag || n % 2 == 0))
    {
        cout << "Yes";
        return;
    }

    cout << "No";
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }

    return 0;
}
