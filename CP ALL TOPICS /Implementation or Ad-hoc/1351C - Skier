#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 1006903069;
const int MAX = 1e7 + 10;

/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

void solve()
{
    int x = 0, y = 0, a, b, i, j;
    map<pair<pair<int, int>, pair<int, int>>, int> mp;
    string s;
    cin >> s;
    int co = 0;
    for (i = 0; i < s.size(); i++)
    {
        int a = x, b = y;
        char c = s[i];
        if (c == 'N')
            y++;
        if (c == 'S')
            y--;
        if (c == 'E')
            x++;
        if (c == 'W')
            x--;
        pair<int, int> p, k;
        p.first = x, p.second = y;
        k.first = a;
        k.second = b;
        pair<pair<int, int>, pair<int, int>> o, h;
        h.first = p;
        h.second = k;
        o.first = k;
        o.second = p;
        if (mp[h] || mp[o])
            co++;
        else
            co += 5;
        mp[h]++;
        mp[o]++;
    }
    cout << co;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    // t = 1;
    while (t--)
    {
        solve();
        cout << endl;
    }

    return 0;
}
