void solve() {
    ll v=0;
    string s,t;
    cin>>s;
    for(int i=0;i<5;i++){
        cin>>t;
        if(s[0]==t[0] || s[1]==t[1])v=1;
    }
    if(v)cout<<"YES";
    else cout<<"NO";
    return;

}


int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL); 
    // int t;cin>>t;
    // while(t--)
        solve();
        cout<<endl;
    
    return 0;   
}
