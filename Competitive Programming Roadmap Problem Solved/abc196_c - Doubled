#include <bits/stdc++.h>

using namespace std;

using ll = long long;

/*
ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    ll n;cin>>n;
    ll ans=0;
    for(ll i=1;i<=1000000;i++){
        string N=to_string(i);
        string T=N+N;
        ll M=stoll(T);
        if(M<=n){
            ans++;
        }
    }
    cout<<ans;

    
   return 0;
    
}
