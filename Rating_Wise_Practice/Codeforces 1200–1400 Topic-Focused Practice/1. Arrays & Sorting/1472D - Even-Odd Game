#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int MOD = 676767677LL;
const int MAX = 1e7 + 5;
/*
//      _   _  __  _   _
_____
_____
//     / | / / / _ \ \ \__\ \\__  _\\  _  \ 
//    /  |/ / / /_\ \ \  _ \  \ \   \ \ \  \ 
//   / /|  / / __  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

void solve(){
 int n;
 cin>>n;
 vector<ll>od,ev;
 for(int i=0; i<n ; i++){
     ll x;
     cin>>x;
     if(x%2==0)ev.push_back(x);
     else od.push_back(x);
 }
 sort(od.begin(),od.end());
 sort(ev.begin(),ev.end());
 
 ll al=0, bo=0;
 
 for(int i=0;i<n;i++){
    int es=ev.size(),os=od.size();
    if(i%2==0){
        if(es>0){
            if(os>0){
                if(od[os-1]>ev[es-1])od.pop_back();
                else{
                    al+=ev[es-1];
                    ev.pop_back();
                }
            }
            else{
                al+=ev[es-1];
                ev.pop_back();
            }
        }
        else{
            od.pop_back();
        }
    }
    else{
        if(os>0){
            if(es>0){
                if(ev[es-1]>od[os-1])ev.pop_back();
                else{
                    bo+=od[os-1];
                    od.pop_back();
                }
            }
            else{
                bo+=od[os-1];
                od.pop_back();
            }
        }
        else ev.pop_back();
    }
 }
 
 if(al>bo)cout<<"Alice";
 else if(al<bo)cout<<"Bob";
 else cout<<"Tie";
}

    

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll t;cin>>t;
   while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
    
