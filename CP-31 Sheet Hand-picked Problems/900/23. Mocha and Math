#include <bits/stdc++.h>
using namespace std;
using ll = long long;

void solve()
{
    int n;
    cin >> n;
    vector<ll> v(n);

    for (auto &x : v)
    {
        cin >> x;
    }
    ll x = v[0];

    for (int i = 1; i < n; i++)
        x &= v[i];
    cout << x;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int Test_Case;
    cin >> Test_Case;
    while (Test_Case--)
    {
        solve();
        cout << endl;
    }
    return 0;
}
