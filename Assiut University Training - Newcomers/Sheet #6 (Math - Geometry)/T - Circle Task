void solve(){
    ll x,y,r,n;
    cin>>x>>y>>r>>n;
    while(n--){
        ll nx,ny;cin>>nx>>ny;
       double dist = sqrt((nx-x)*(nx-x) + (ny-y)*(ny-y) +0.0);
       if(dist>r)cout<<"NO";
       else cout<<"YES";
       cout<<endl;
    }
}

