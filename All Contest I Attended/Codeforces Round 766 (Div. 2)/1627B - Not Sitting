#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 10000;
const ll MAXN = 2e5 + 5;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
// 
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

void solve() {
    int n, m;
    cin >> n >> m;
    vector<int> v;
    
    for (int i = 0; i < n; ++i)
        for (int j = 0; j < m; ++j)
            v.push_back(max(i, n - i - 1) + max(j, m - j - 1));
    
    sort(v.begin(), v.end());
    for (int i = 0; i < n * m; ++i)
        cout << v[i] << " ";
    
    cout << "\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    
    int t;
    cin >> t;
    while (t--)
        solve();
    
    return 0;
}
