void solve(){

int n;
cin >>n;
vector<int>a(n);
bool z,otn,tn,ntn,hun;
int ans=0;
z=otn=tn=ntn=hun=false;
int otnv=-1,ntnv=-1,tnv=-1;
for(int i=0;i<n;i++){
    cin>>a[i];
    if(a[i]==0)z=true;
    else if(a[i]>0 && a[i]<10){
        otn=true;otnv=a[i];
    }
    else if(a[i]==100)hun=true;
    else if( a[i]%10==0){
        tnv=a[i];tn=true;
    }
    else{
        ntn=true;
        ntnv=a[i];
    }

}
if(z){
    ans++;
}

if(otn){
    ans++;
}
if(tn)ans++;
if(hun)ans++;

if(!otn && !tn && ntn)ans++;

cout<<ans<<endl;

if(z)cout<<0<<" ";
if(otn)cout<<otnv<<" ";
if(tn)cout<<tnv<<" ";
if(hun)cout<<100<<" ";
if(!otn && !tn && ntn)cout<<ntnv;
}


    

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
//    ll t;cin>>t;
//    while(t--){
        solve();
//         cout<<endl;
//    }
return 0;
}
    
