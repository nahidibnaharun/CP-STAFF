#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 998244353;
const int MAXV = 5000000;

/*
    _   __  ___  __   __ _______ _______
   / | / / / _ \ \ \__\ \\__  __\\  __  \
  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
*/

void solve()
{
    int t;
    cin >> t;

    vector<int> queries(t);
    int qmax = 0;

    for (int i = 0; i < t; i++)
    {
        cin >> queries[i];
        qmax = max(qmax, queries[i]);
    }

    if (qmax == 0)
        return;

    vector<int> spf(MAXV + 1);
    vector<int> primes;
    primes.reserve(350000);

    spf[1] = 1;

    for (int i = 2; i <= MAXV; i++)
    {
        if (spf[i] == 0)
        {
            spf[i] = i;
            primes.push_back(i);
        }
        for (int p : primes)
        {
            long long v = 1LL * p * i;
            if (v > MAXV)
                break;
            spf[v] = p;
            if (p == spf[i])
                break;
        }
    }

    vector<char> used(MAXV + 1, 0);
    vector<int> nextMult(MAXV + 1, 0);

    for (int p : primes)
        nextMult[p] = p;

    vector<int> seq(qmax + 1);
    seq[1] = 2;
    used[2] = 1;

    for (int i = 1; i < qmax; i++)
    {
        int x = seq[i];
        int y = x;
        int lastp = 0;
        int best = INT_MAX;

        while (y > 1)
        {
            int p = spf[y];
            y /= p;
            if (p == lastp)
                continue;
            lastp = p;

            int &ptr = nextMult[p];
            while (ptr <= MAXV && used[ptr])
                ptr += p;

            if (ptr <= MAXV && ptr < best)
                best = ptr;
        }

        seq[i + 1] = best;
        used[best] = 1;
    }

    for (int i = 0; i < t; i++)
        cout << seq[queries[i]] << endl;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    solve();

    return 0;
}
