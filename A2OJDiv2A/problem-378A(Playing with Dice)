#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b;
    cin>>a>>b;
    int cna=0,d=0,cnb=0;
    for(int i=1;i<=6;i++){
        int difa= abs(i-a),difb=abs(i-b);
        if(difa<difb ) cna++;
        else if(difb<difa) cnb++;
        else d++;
    }
    cout<<cna<<" "<<d<<" "<<cnb;
    return 0;
}
