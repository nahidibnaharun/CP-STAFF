 void solve(){
int n;cin>>n;
int cnt=0;
int x=1,y=n-1;
while(y>0){
    if(y%x==0)cnt++;
    x++;y--;
}cout<<cnt;
    cout<<endl;
 }
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    // int t;cin>>t;
    // while(t--)
    solve();
}
