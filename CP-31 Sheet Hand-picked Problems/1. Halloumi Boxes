int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    // freopen("speeding.in", "r", stdin); 
    // freopen("speeding.out", "w", stdout); 
    int t;
    cin>>t;
    while(t--){
	int n,k;
    cin>>n>>k;
    int a[n];
    for(int i=0;i<n;i++)cin>>a[i];
    if(is_sorted(a,a+n)||k>1)cout<<"YES";
    else cout<<"NO";
    cout<<endl;
    }
	return 0;
}
