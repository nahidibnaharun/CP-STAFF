void solve(){
int a,b,c;cin>>a>>b>>c;
if((a+b)>=10 || (b+c)>=10 || (c+a)>=10)cout<<"YES";
else cout<<"NO";
cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
    int t;cin>>t;
    while(t--)
    solve();
   return 0;   
}


