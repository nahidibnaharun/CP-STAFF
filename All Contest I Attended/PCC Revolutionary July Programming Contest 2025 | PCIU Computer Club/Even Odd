#include <bits/stdc++.h>
using namespace std;
using ll = long long;

void solve(){

 int n;cin>>n;
 int p=0;string s;
 for(int i=0;i<n;i++){
    cin>>s;char l=s.back();
    if((l-'0')%2==1)p^=1;
 }
if(p)cout<<"Odd";
else cout<<"Even";

}
int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    // int t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
    return 0;
}
