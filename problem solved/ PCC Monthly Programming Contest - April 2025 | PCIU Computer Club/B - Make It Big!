#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const int N= 1e5+10;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/

 void solve(){
   string n;cin>>n;
   string mxn=n;
   for(int i=0;i<n.size();i++){
    for(int j=i+1;j<n.size();j++){
        swap(n[i],n[j]);
        for(int k=0;k<n.size();k++){
            for(int l=k+1;l<n.size();l++){
                swap(n[k],n[l]);mxn=max(mxn,n);
                swap(n[k],n[l]);
            }
        }swap(n[i],n[j]);
    }
   }cout<<mxn<<endl;
 }


int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    int t;cin>>t;
    while(t--)
    solve();

       
}

