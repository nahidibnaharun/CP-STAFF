vector<int>v;
void f(int i, int n){
    if(n==0)return;
    int x;cin>>x;
    if(i%2==0) v.push_back(x);
    f(i+1,n-1);
}
void solve(){
int n;cin>>n;
int i=0;f(i,n);
for(int i=v.size()-1;i>-1;i--)cout<<v[i]<<" ";
v.clear();
 }

