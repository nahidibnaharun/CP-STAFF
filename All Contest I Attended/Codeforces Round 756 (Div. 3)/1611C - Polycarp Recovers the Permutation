void solve(){
int n;cin>>n;vector<int>a(n);
for(int i=0;i<n;i++)cin>>a[i];
if(a[0]!=n && a[n-1]!=n)cout<<-1<<endl;
else{
    for(int i=n-1;i>=0 ;i--)cout<<a[i]<<" ";
    cout<<endl;
}
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  int t;cin>>t;
  while(t--){
    solve();
    // cout<<endl;
  }
}
