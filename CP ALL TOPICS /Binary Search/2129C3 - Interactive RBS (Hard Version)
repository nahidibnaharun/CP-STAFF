#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const int INF=500;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
int t,n,pl,pr,ans[1005],g[13],f[13],res;
int ask(const vector<int>&v){
    cout<<"? "<<v.size();
    for(int x:v)cout<<" "<<x;
    cout<<endl<<flush;
    int r;cin>>r;return r;
}
void solve(){
    cin>>n;memset(ans,0,sizeof(ans));
    int l=2,r=n,pos=0;
    while(l<=r){
        int m=(l+r)>>1;
        vector<int>q(m);
        iota(q.begin(),q.end(),1);
        if(ask(q))pos=m,r=m-1;
        else l=m+1;
    }
    if(!pos)pl=n,pr=1;
    else pl=pos-1,pr=pos;
    int i=1;
    for(;i+11<=n;i+=12){
        vector<int>q;q.reserve(2*1000);
        for(int j=0;j<12;++j){
            int idx=i+j,id=j+1;
            for(int idt=0;idt<g[id];++idt){
                q.push_back(idx),q.push_back(pr);
            }q.push_back(pr);
        }
        int v=ask(q);
        for(int j=11;j>=0;--j){
            int idx=i+j,id=j+1;
            if(v>=f[id]){v-=f[id];ans[idx]=1;}
            else ans[idx]=2;
        }
    }
    for(;i+1<=n;i+=2){
        int x=i,y=i+1;
        vector<int>q={x,pr,y,pr,pl,pr};
        int v=ask(q);
        if(v==1)ans[x]=ans[y]=2;
        else if(v==2)ans[x]=1,ans[y]=2;
        else if(v==3)ans[x]=2,ans[y]=1;
        else ans[x]=ans[y]=1;
    }
    if(i==n){
        int x=n-1,y=n;
        vector<int>q={x,pr,y,pr,pl,pr};
        int v=ask(q);
        if(v==1) ans[x]=ans[y]=2;
        else if(v==2)ans[x]=1,ans[y]=2;
        else if(v==3)ans[x]=2,ans[y]=1;
        else ans[x]=ans[y]=1;
    }
    cout<<"! ";
    for(int j=1;j<=n;++j)cout<<(ans[j]==1?"(":")");
    cout<<endl<<flush;
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    g[1]=f[1]=res=1;
    for(int i=2,cur=1,pref=1;i<=12;++i){
        while(1LL*cur*(cur+1)/2<=pref)++cur;
        g[i]=cur;f[i]=cur*(cur+1)/2;
        pref+=f[i];
        res+=cur;
    }
    res=res*2+12;
    cin>>t;
    while(t--){
    solve();
    // cout<<endl;
    }
}
