void solve(){
	int n;
	cin>>n;


}

int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int t;
	cin>>t;
	while(t--){
		int a;
		cin>>a;
		string s1,s2;
		cin>>s1>>s2;
		int o=0,z=0;
		for(int i=0;i<a;i++){
		    if(s1[i]=='1' && s2[i] =='1')o++;
            else if(s1[i]=='1' || s2[i]=='1')z++;
		}
		if(o%2==1 ){
		    cout<<"YES";
		}
		else if (o%2==0 && z>0) cout<<"YES";
		else cout<<"NO";
		cout<<endl;
	}

}
