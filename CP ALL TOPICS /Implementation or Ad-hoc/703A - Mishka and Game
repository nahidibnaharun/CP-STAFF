int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("tracing.in", "r", stdin); 
    // freopen("tracing.out", "w", stdout); 
    int t;cin>>t;
    int m=0,c=0;
    while(t--){
        int a,b;
        cin>>a>>b;
        if(a>b)m++;
        else if(a<b)c++;
    }
    if(m==c)cout<<"Friendship is magic!^^";
    else if(m>c)cout<<"Mishka";
    else cout<<"Chris";
   return 0;   
}


