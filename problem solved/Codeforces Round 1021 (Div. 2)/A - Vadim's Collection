 void solve(){
   
    cout<<endl;
 }
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    const vector<int> req={9,8,7,6,5,4,3,2,1,0};
    int t;cin>>t;
    while(t--){
        string s;cin>>s;
        int cnt[10]={};
        for(char c:s)cnt[c-'0']++;
        string ans;
        ans.reserve(10);
        for(int i=0;i<10;i++){
            for(int d=req[i];d<10;d++){
                if(cnt[d]>0){
                    ans.push_back('0'+d);
                    cnt[d]--;
                    break;
                }
            }
        }
        cout<<ans<<endl;
    }

       
}


