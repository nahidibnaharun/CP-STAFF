#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
const int INF=500;
/*
//      _   __  ___  __   __ _______ _______
//     / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//    /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//   / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){  
    int n;cin>>n;vector<vector<int>>a(n,vector<int>(n));
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cin >> a[i][j];
        }
    }
 
    int ans = 0;
 
    vector<int> c(n), x(n);
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            c[j] ^= 1;
            if (c[j]) {
                ans ^= a[i][j];
            }
        }
        swap(c, x);
        for (int j = 0; j < n; j++) {
            if (j < n - 1) {
                c[j + 1] ^= x[j];
            }
            if (j > 0) {
                c[j - 1] ^= x[j];
            }
        }
    }
    cout<<ans;
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
    solve();
    cout<<endl;
    }
}
