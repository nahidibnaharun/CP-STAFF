
#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const int INF = 1e9;
const int MOD = 1e9+7;
/*

         _   _  __    _   _  _____   _____
    / | / / / _ \ \ \__\ \\__  _\\  _   \
   /  |/ / / /_\ \ \  __  \  \ \  \ \ \  \
  / /|  / / ____  \ \ \  \ _\_\ \__\ \/  /
 /_/ |_/ /_/     \__\\_\  \_\\______\\___/
 
ID: mdnahidibnaharun
LANG: C++ 
TASK: Codeforces Round 1061 (Div. 2)
*/



void solve(){
    
    int n; cin >> n;
    string b="";
    while(n)b.push_back((n%2)+'0'),n/=2;

    reverse(b.begin() , b.end());
    string ans="NO";
    for(int i=1 ;i<64;i++){
        if(b.size() <=i){
            string w=string(i-b.size() ,'0')+b;
            bool chk=1;
            for(int j=0 ;j<i/2 ;j++)if(w[j]!=w[i-1-j])chk=0;
            if(i%2 && w[i/2] == '1')chk=0;
            if(chk)ans="YES";
        }
    }
    cout<<ans;
}


int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int Test_Case;
    cin >> Test_Case;
    while(Test_Case--){
        solve();
        cout<<endl;
    }
    return 0;
}
