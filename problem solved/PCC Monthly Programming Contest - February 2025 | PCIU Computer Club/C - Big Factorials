int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    //  freopen("badmilk.in", "r", stdin); 
    // freopen("badmilk.out", "w", stdout); 
   int n;
   cin>>n;
   ll fm = 1;
   for(int i=1;i<=n;i++){
    fm = (fm *i)%10000;
   }

   if( n>=7)cout<<setw(4)<<setfill('0')<<fm;
   else cout<<fm;
    
    return 0;   
}
