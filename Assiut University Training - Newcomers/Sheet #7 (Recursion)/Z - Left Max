void fun(ll mx,vector<ll> &a,int n,int i){
    if(i>=n)return;

    mx=max(mx,a[i]);
    cout<<mx<<" ";
    fun(mx,a,n,i+1);

}
void solve(){
    int n;cin>>n;
    vector<ll>a(n);
    for(auto &x:a)cin>>x;
    fun(-1e10,a,n,0);
}
int main(){
    ios_base::sync_with_stdio(false);
