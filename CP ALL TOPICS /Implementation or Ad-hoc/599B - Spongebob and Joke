#include <bits/stdc++.h>
using namespace std;
using ll = long long;

void solve(){
int n,m;cin>>n>>m;vector<int>f(n),b(m);
for(auto &x:f)cin>>x;
for(auto &x:b)cin>>x;
unordered_map<int,deque<int>>fh;
for(int i=1;i<=n;i++)fh[f[i-1]].push_back(i);
for(int i:b){
    if(fh[i].size()==0){cout<<"Impossible";return;}
}
deque<int>rs;
for(int i:b){
    if(fh[i].size()>1){cout<<"Ambiguity";return;}
    rs.push_back(fh[i][0]);
}
cout<<"Possible"<<endl;
for(int i=0;i<((int)rs.size()-1);i++)cout<<rs[i]<<" ";
cout<<rs[rs.size()-1]<<endl;
}
int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    // int t;cin>>t;
    // while(t--){
        solve();
    //     cout<<endl;
    // }
    return 0;
}
