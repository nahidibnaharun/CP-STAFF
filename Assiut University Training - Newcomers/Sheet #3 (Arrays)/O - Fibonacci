void solve(){
int n;cin>>n;
if(n==1)cout<<0;
else if(n==2)cout<<1;
else{
ll a=1,b=0;
for(int i=1;i<n-1;i++){
 ll tm=a;
   a=a+b;
     b=tm; 
}
cout<<a;
}
}
    
