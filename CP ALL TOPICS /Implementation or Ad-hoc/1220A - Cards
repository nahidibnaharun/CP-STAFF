void solve(){
int n;string s;cin>>n>>s;
for(int i=0;i<=n;i++){
    if(s[i]=='n')cout<<1<<" ";
}
for(int j=0;j<=n;j++){
    if(s[j]=='z')cout<<0<<" ";
}
cout<<endl;
}
   
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
//   int t;cin>>t;
//   while(t--)
    solve();
} 
