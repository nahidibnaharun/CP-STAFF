#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \ 
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \ 
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
 void solve(){
 int n;cin>>n;
 vector<int>v;
 for(int j=0;j<n;j++){
    string s;
    cin>>s;
    for(int i=0;i<4;i++)if(s[i]=='#')v.push_back(i+1);
 }
 for(int i=n-1;i>-1;i--)cout<<v[i]<<" ";
    cout<<endl;
 }
int main(){
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    int t;cin>>t;
    while(t--)
    solve();
}
