void solve(){
    ll x1,y1,x2,y2;
    cin>>x1>>y1>>x2>>y2;
    x1=abs(x1-x2),y2=abs(y2-y1);
    double ans=sqrt(x1*x1*1.0 + y2*y2*1.0);
    cout<<fixed<<setprecision(9)<<ans;
}

