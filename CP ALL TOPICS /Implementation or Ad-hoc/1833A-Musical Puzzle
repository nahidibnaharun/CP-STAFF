void solve(){
  ll n,cnt=0;
  cin>>n;
  string s;cin>>s;
  set<string>x;
  for(int i=0;i<n-1;i++){
    string tm;
    tm+=s[i];tm+=s[i+1];
    x.insert(tm);
  }
  cout<<x.size();
cout<<endl;
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--)
    solve();
}
