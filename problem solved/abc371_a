*/

void solve() {
char a,b,c;
cin>>a>>b>>c;
if(a=='<'){
	if(b=='<'){
		if(c=='<'){
			cout<<"B";
		}
		else{
			cout<<'C';
		}
	}
	else{
		cout<<"A";
	}
}
else{
	if(b=='<'){
			cout<<"A";
	}
	else{
		if(c=='<'){
			cout<<"C";
		}
		else{
			cout<<'B';
		}
	}
}
cout<<endl;
return ;
}

int main() {
 
   solve();
 

  return 0;
}
