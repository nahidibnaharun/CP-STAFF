#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 1e9 + 7;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

void solve()
{
  string s, t;
  cin >> s >> t;
  int a[26] = {0};
  for (auto x : t)
    a[x - 'A']++;

  string ans;

  for (int i = (int)s.size() - 1; i >= 0; i--)
  {
    if (a[s[i] - 'A'])
    {
      ans += s[i];
      a[s[i] - 'A']--;
    }
  }
  reverse(ans.begin(), ans.end());

  cout << (ans == t ? "YES" : "NO");
}
int main()
{
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

  ll Test_Case;
  cin >> Test_Case;
  while (Test_Case--)
  {
    solve();
    cout << endl;
  }

  return 0;
}
