#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ui = unsigned int;
const ll INF = 1e17;
/*
//      _   _  __  _   _
_____
_____
//     / | / / / _ \ \ \__\ \\__  _\\  _  \ 
//    /  |/ / / /_\ \ \  _ \  \ \   \ \ \  \ 
//   / /|  / / __  \ \ \  \ \ _\ \___\ \/  /
//  /_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice 
*/
void solve(){
    string s;
    cin >> s;
    int fr[26];
		memset(fr, 0, sizeof(fr));
		for (auto c : s) fr[c - 'a']++;
		int c1=0,c2=0;
		for (int i = 0; i < 26; i++)
			if (fr[i] == 1)
				c1++;
			else if (fr[i] > 0)
				c2++;
		cout << (c2 + c1 / 2);
 }
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;cin>>t;
    while(t--){
        solve();
        cout<<endl;
    }
return 0;
}
