void solve(){
    string s;cin>>s;
    int ar[26]={0};
    for(auto x:s)ar[x-'a']++;
    int cnt=0;
    for(auto x:ar)if(x>0)cnt++;
    if(cnt%2==0)cout<<"CHAT WITH HER!";
    else cout<<"IGNORE HIM!";
}
