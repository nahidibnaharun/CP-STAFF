void solve()
{

    int n;
    cin >> n;
    vector<int> a(n);
    for (auto &i : a)
        cin >> i;
    sort(a.begin() + 1, a.end());
    cout << (a[0] > a[1] ? "Alice" : "Bob") << endl;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        // cout << endl;
    }

    return 0;
}
