void solve(){
    int n,cnt=0;ll x;cin>>n>>x;
   while(n--){
    char a;
    ll d;
    cin>>a>>d;
    if(a=='+')x+=d;
    else if(a=='-'){
        if(x>=d){
            x-=d;
        }
        else cnt++;
    }
   }
   cout<<x<<" "<<cnt;

}
int main(){
    ios_base::sync_with_stdio(false);
