#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 998244353;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

void solve() {
  int n;
  cin >> n;
  string s;
  cin >> s;
  vector<int> a, b;
  for (int i = 0; i < n; i++) {
    if (s[i] == 'T')
      a.push_back(i);
    else
      b.push_back(i);
  }
  if (a.size() != 2 * b.size()) {
    cout << "NO";
    return;
  }
  for (int i = 0; i < b.size(); i++) {
    if (b[i] < a[i] || b[i] > a[i + b.size()]) {
      cout << "NO";
      return;
    }
  }
  cout << "YES";
}

int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

  ll Test_Case;
  cin >> Test_Case;
  while (Test_Case--) {
    solve();
    cout << endl;
  }

  return 0;
}
