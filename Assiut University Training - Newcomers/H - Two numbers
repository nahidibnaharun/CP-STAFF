void solve(){
int a,b;cin>>a>>b;
int f1=a/b;
cout<<"floor "<<a<<" / "<<b<<" = "<<f1<<endl;
double x=(1.0)*a/b;
if((x-f1)*10<5)x=f1;
else x=f1+1;
if(a%b!=0)f1++;
cout<<"ceil "<<a<<" / "<<b<<" = "<<f1<<endl;
cout<<"round "<<a<<" / "<<b<<" = "<<x<<endl;
 }

