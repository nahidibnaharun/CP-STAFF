bool fun( string x , vector<string> s){
    for(auto st : s){
    int posx = x.find(st[0]), posy = x.find(st[2]);
    if(st[1]=='>' && posx < posy)return false;
    if(st[1] == '<' && posx > posy)return false;
    }
    return true;

}

void solve(){
    vector<string> s;
    for(int i=0;i<3;i++){
        string x;
        cin >> x;
        s.push_back(x);
    }
    vector<string> correct ={"ABC","ACB", "CAB","CBA","BAC","BCA"};
    for(auto x : correct){
        if(fun(x , s)){
            cout << x;
            return;
        }
    }
    cout <<"Impossible";
}


