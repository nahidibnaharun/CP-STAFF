#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;
const int INF = 1e9;
const int SIZE = 1e5;
const int MOD = 1e9 + 7;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/

ID: mdnahidibnaharun
LANG: C++
TASK: practice
*/

vector<int> primes;
vector<bool> is_prime(SIZE + 1, true);

void sieve()
{
  is_prime[0] = is_prime[1] = false;

  for (int i = 2; i <= SIZE; i++)
  {
    if (is_prime[i])
    {
      primes.push_back(i);

      for (int j = i + i; j <= SIZE; j += i)
        is_prime[j] = false;
    }
  }
}
void solve()
{
  int n;
  cin >> n;
  vector<int> fa(n + 1, 0), fb(n + 1, 0);

  for(int i=0 ;i<n;i++){
    int x; cin >> x;
    fa[x]++;
  }

  for(int i=0 ;i<n;i++){
    int x; cin >> x;
    fb[x]++;
  }
  vector<ll> ca(n + 1, 0), cb(n + 1, 0);

  for (int i = 1; i <= n; i++)
  {
    for (int m = i; m <= n; m += i)
    {
      ca[i] += fa[m];
      cb[i] += fb[m];
    }
  }

  vector<ll> ans(n + 1, 0);

  for (int i = n; i >= 1; i--)
  {
    ll total = ca[i] * cb[i];

    for (int m = 2 * i; m <= n; m += i)
      total -= ans[m];

    ans[i] = total;
  }

  ll ans1 = 0;
  for (auto x : primes)
  {
    if (x > n)
      break;
    ans1 += ans[x];
  }

  cout << ans1;
}
int main()
{
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

  sieve();

  ll Test_Case;
  cin >> Test_Case;
  while (Test_Case--)
  {
    solve();
    cout << endl;
  }

  return 0;
}
