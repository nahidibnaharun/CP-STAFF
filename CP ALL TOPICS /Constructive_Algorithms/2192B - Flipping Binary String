#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ull = unsigned long long;
using ld = long double;
using ui = unsigned int;

const int INF = 1e9;
const int MOD = 1000000007;
const int MAX = 10000;
const ll MAXN = 2e5 + 5;
/*
//	    _   __  ___  __   __ _______ _______
//	   / | / / / _ \ \ \__\ \\__  __\\  __  \
//	  /  |/ / / /_\ \ \  ___ \  \ \   \ \ \  \
//	 / /|  / / ____  \ \ \  \ \ _\ \___\ \/  /
//	/_/ |_/ /_/    \__\ \_\  \_\\______\\___/
//
// ID: mdnahidibnaharun
// LANG: C++
// TASK: practice
*/

void solve()
{

    ll n;
    cin >> n;

    string a;
    cin >> a;

    ll c0 = 0;

    for (ll i = 0; i < n; i++)
    {
        if (a[i] == '0')
        {
            c0++;
        }
    }

    ll c1 = n - c0;

    if (c0 % 2)
    {
        cout << c0 << endl;
        for (ll i = 0; i < n; i++)
        {
            if (a[i] == '0')
            {
                cout << i + 1 << " ";
            }
        }
    }
    else if ((c1 % 2) == 0)
    {
        cout << c1 << endl;
        if (c1 != 0)
        {
            for (ll i = 0; i < n; i++)
            {
                if (a[i] == '1')
                {
                    cout << i + 1 << " ";
                }
            }
        }
    }
    else
    {
        cout << -1;
    }
    cout << endl;
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
    {
        solve();
        // cout << endl;
    }
    return 0;
}
