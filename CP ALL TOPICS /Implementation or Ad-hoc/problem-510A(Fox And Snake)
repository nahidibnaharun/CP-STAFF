#include <bits/stdc++.h>

using namespace std;

int main()
{int a,b;
cin>>a>>b;
int k=b-1;
for(int i=0;i<a;i++){
    for(int j=0;j<b;j++){
        if(i%2==0 || j==k )
        cout<<"#";
        else cout<<".";
    }
    if(i%2!=0 ) {
        if(k==b-1) k=0;
        else k=b-1;
    }
    cout<<endl;
}
    return 0;
}
