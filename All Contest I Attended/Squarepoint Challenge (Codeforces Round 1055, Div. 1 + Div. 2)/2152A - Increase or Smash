void solve(){
    int n; cin >> n;
    set<int>s;
    for(int i=0 ;i<n;i++){
        int l; cin >> l ; s.insert(l);
    }
    cout << s.size()*2-1;
}



int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int Test_Case;
    cin >> Test_Case;
    while(Test_Case--){
        solve();
        cout<<endl;
    }
    return 0;
}
